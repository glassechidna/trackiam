{
  "Arn": "arn:aws:iam::aws:policy/AmazonSageMakerTrainingPlanCreateAccess",
  "CreateDate": "2024-12-04T13:21:07Z",
  "DefaultVersionId": "v2",
  "IsAttachable": true,
  "Path": "/",
  "PolicyId": "ANPAZKAPJZG4FCUDPFEIQ",
  "PolicyName": "AmazonSageMakerTrainingPlanCreateAccess",
  "PolicyVersion": {
    "CreateDate": "2025-08-08T21:19:07Z",
    "Document": {
      "Statement": [
        {
          "Action": [
            "sagemaker:AddTags"
          ],
          "Condition": {
            "StringEquals": {
              "sagemaker:TaggingAction": [
                "CreateReservedCapacity",
                "CreateTrainingPlan"
              ]
            }
          },
          "Effect": "Allow",
          "Resource": [
            "arn:aws:sagemaker:*:*:reserved-capacity/*",
            "arn:aws:sagemaker:*:*:training-plan/*"
          ],
          "Sid": "AddTagsToTrainingPlanPermissions"
        },
        {
          "Action": "sagemaker:DescribeTrainingPlan",
          "Effect": "Allow",
          "Resource": [
            "arn:aws:sagemaker:*:*:training-plan/*"
          ],
          "Sid": "DescribeTrainingPlanPermissions"
        },
        {
          "Action": "sagemaker:ListUltraServersByReservedCapacity",
          "Effect": "Allow",
          "Resource": [
            "arn:aws:sagemaker:*:*:reserved-capacity/*"
          ],
          "Sid": "ListUltraServersByReservedCapacityPermissions"
        },
        {
          "Action": [
            "sagemaker:CreateReservedCapacity",
            "sagemaker:CreateTrainingPlan",
            "sagemaker:DescribeReservedCapacity"
          ],
          "Effect": "Allow",
          "Resource": [
            "arn:aws:sagemaker:*:*:reserved-capacity/*",
            "arn:aws:sagemaker:*:*:training-plan/*"
          ],
          "Sid": "CreateTrainingPlanPermissions"
        },
        {
          "Action": [
            "sagemaker:ListTrainingPlans",
            "sagemaker:SearchTrainingPlanOfferings"
          ],
          "Effect": "Allow",
          "Resource": "*",
          "Sid": "NonResourceLevelTrainingPlanPermissions"
        }
      ],
      "Version": "2012-10-17"
    },
    "IsDefaultVersion": true,
    "VersionId": "v2"
  },
  "UpdateDate": "2025-08-08T21:19:07Z"
}
