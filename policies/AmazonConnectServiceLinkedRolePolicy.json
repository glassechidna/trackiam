{
  "Arn": "arn:aws:iam::aws:policy/aws-service-role/AmazonConnectServiceLinkedRolePolicy",
  "CreateDate": "2018-09-07T00:21:43Z",
  "DefaultVersionId": "v9",
  "IsAttachable": true,
  "Path": "/aws-service-role/",
  "PolicyId": "ANPAJ6R6FMTSRUJSKI72Y",
  "PolicyName": "AmazonConnectServiceLinkedRolePolicy",
  "PolicyVersion": {
    "CreateDate": "2023-09-27T21:05:04Z",
    "Document": {
      "Statement": [
        {
          "Action": "cloudwatch:PutMetricData",
          "Condition": {
            "StringEquals": {
              "cloudwatch:namespace": "AWS/Connect"
            }
          },
          "Effect": "Allow",
          "Resource": "*"
        },
        {
          "Action": [
            "wisdom:CreateContent",
            "wisdom:CreateKnowledgeBase",
            "wisdom:CreateSession",
            "wisdom:DeleteContent",
            "wisdom:GetAssistant",
            "wisdom:GetContent",
            "wisdom:GetKnowledgeBase",
            "wisdom:GetRecommendations",
            "wisdom:GetSession",
            "wisdom:NotifyRecommendationsReceived",
            "wisdom:QueryAssistant",
            "wisdom:StartContentUpload",
            "wisdom:TagResource",
            "wisdom:UntagResource",
            "wisdom:UpdateContent"
          ],
          "Condition": {
            "StringEquals": {
              "aws:ResourceTag/AmazonConnectEnabled": "True"
            }
          },
          "Effect": "Allow",
          "Resource": "*"
        },
        {
          "Action": [
            "connect:*"
          ],
          "Effect": "Allow",
          "Resource": [
            "*"
          ]
        },
        {
          "Action": [
            "iam:DeleteRole"
          ],
          "Effect": "Allow",
          "Resource": "arn:aws:iam::*:role/aws-service-role/connect.amazonaws.com/AWSServiceRoleForAmazonConnect_*"
        },
        {
          "Action": [
            "lex:ListBotAliases",
            "lex:ListBots"
          ],
          "Effect": "Allow",
          "Resource": "*"
        },
        {
          "Action": [
            "logs:CreateLogStream",
            "logs:DescribeLogStreams",
            "logs:PutLogEvents"
          ],
          "Effect": "Allow",
          "Resource": [
            "arn:aws:logs:*:*:log-group:/aws/connect/*:*"
          ]
        },
        {
          "Action": [
            "profile:AddProfileKey",
            "profile:CreateProfile",
            "profile:ListProfileObjectTypes",
            "profile:SearchProfiles",
            "profile:UpdateProfile"
          ],
          "Effect": "Allow",
          "Resource": "arn:aws:profile:*:*:domains/amazon-connect-*"
        },
        {
          "Action": [
            "profile:GetProfileObjectType",
            "profile:ListProfileObjects"
          ],
          "Effect": "Allow",
          "Resource": [
            "arn:aws:profile:*:*:domains/amazon-connect-*/object-types/*"
          ]
        },
        {
          "Action": [
            "profile:GetProfileObjectTypeTemplate",
            "profile:ListProfileObjectTypeTemplates"
          ],
          "Effect": "Allow",
          "Resource": "arn:aws:profile:*:*:/templates*"
        },
        {
          "Action": [
            "profile:ListAccountIntegrations"
          ],
          "Effect": "Allow",
          "Resource": "*"
        },
        {
          "Action": [
            "s3:DeleteObject",
            "s3:GetObject",
            "s3:GetObjectAcl",
            "s3:PutObject",
            "s3:PutObjectAcl"
          ],
          "Effect": "Allow",
          "Resource": [
            "arn:aws:s3:::amazon-connect-*/*"
          ]
        },
        {
          "Action": [
            "s3:GetBucketAcl",
            "s3:GetBucketLocation"
          ],
          "Effect": "Allow",
          "Resource": [
            "arn:aws:s3:::amazon-connect-*"
          ]
        },
        {
          "Action": [
            "wisdom:ListAssistants",
            "wisdom:ListKnowledgeBases"
          ],
          "Effect": "Allow",
          "Resource": "*"
        }
      ],
      "Version": "2012-10-17"
    },
    "IsDefaultVersion": true,
    "VersionId": "v9"
  },
  "UpdateDate": "2023-09-27T21:05:04Z"
}
