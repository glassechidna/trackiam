{
  "Arn": "arn:aws:iam::aws:policy/AWSEntityResolutionConsoleFullAccess",
  "CreateDate": "2023-08-17T17:54:14Z",
  "DefaultVersionId": "v5",
  "IsAttachable": true,
  "Path": "/",
  "PolicyId": "ANPAZKAPJZG4HPFJ6XDUE",
  "PolicyName": "AWSEntityResolutionConsoleFullAccess",
  "PolicyVersion": {
    "CreateDate": "2026-02-12T18:02:20Z",
    "Document": {
      "Statement": [
        {
          "Action": [
            "iam:PassRole"
          ],
          "Condition": {
            "StringEquals": {
              "iam:PassedToService": [
                "entityresolution.amazonaws.com"
              ]
            }
          },
          "Effect": "Allow",
          "Resource": "arn:aws:iam::*:role/*entityresolution*",
          "Sid": "PassRoleToEntityResolutionService"
        },
        {
          "Action": [
            "connect:ListInstances",
            "profile:GetDomain",
            "profile:ListAccountIntegrations",
            "profile:ListDomains",
            "profile:ListIntegrations",
            "profile:ListProfileObjectTypes"
          ],
          "Effect": "Allow",
          "Resource": "*",
          "Sid": "CustomerProfilesIntegrationReadAccess"
        },
        {
          "Action": [
            "dataexchange:GetDataSet"
          ],
          "Effect": "Allow",
          "Resource": "*",
          "Sid": "ADXReadAccess"
        },
        {
          "Action": [
            "entityresolution:*"
          ],
          "Effect": "Allow",
          "Resource": "*",
          "Sid": "EntityResolutionAccess"
        },
        {
          "Action": [
            "events:DeleteRule",
            "events:PutRule",
            "events:PutTargets"
          ],
          "Effect": "Allow",
          "Resource": [
            "arn:aws:events:*:*:rule/entity-resolution-automatic*"
          ],
          "Sid": "ManageEventBridgeRules"
        },
        {
          "Action": [
            "glue:GetDatabase",
            "glue:GetDatabases",
            "glue:GetSchema",
            "glue:GetSchemaByDefinition",
            "glue:GetSchemaVersion",
            "glue:GetSchemaVersionsDiff",
            "glue:GetTable",
            "glue:GetTableVersion",
            "glue:GetTableVersions",
            "glue:GetTables",
            "glue:SearchTables"
          ],
          "Effect": "Allow",
          "Resource": "*",
          "Sid": "GlueSourcesConsoleDisplay"
        },
        {
          "Action": [
            "iam:ListRoles"
          ],
          "Effect": "Allow",
          "Resource": "*",
          "Sid": "ListRolesToPickRoleForPassing"
        },
        {
          "Action": [
            "kms:DescribeKey",
            "kms:ListAliases"
          ],
          "Effect": "Allow",
          "Resource": "*",
          "Sid": "KMSConsoleDisplay"
        },
        {
          "Action": [
            "profile:PutProfileObjectType"
          ],
          "Effect": "Allow",
          "Resource": [
            "arn:aws:profile:*:*:domains/*/object-types/*"
          ],
          "Sid": "CustomerProfilesIntegrationWriteAccess"
        },
        {
          "Action": [
            "s3:GetBucketLocation",
            "s3:GetBucketVersioning",
            "s3:ListBucket",
            "s3:ListBucketVersions"
          ],
          "Effect": "Allow",
          "Resource": "*",
          "Sid": "S3SourcesConsoleDisplay"
        },
        {
          "Action": [
            "s3:ListAllMyBuckets"
          ],
          "Effect": "Allow",
          "Resource": "*",
          "Sid": "S3BucketsConsoleDisplay"
        },
        {
          "Action": [
            "tag:GetTagKeys",
            "tag:GetTagValues"
          ],
          "Effect": "Allow",
          "Resource": "*",
          "Sid": "TaggingConsoleDisplay"
        }
      ],
      "Version": "2012-10-17"
    },
    "IsDefaultVersion": true,
    "VersionId": "v5"
  },
  "UpdateDate": "2026-02-12T18:02:20Z"
}
