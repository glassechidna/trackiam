{
  "Arn": "arn:aws:iam::aws:policy/AmazonS3TablesFullAccess",
  "CreateDate": "2024-12-03T15:21:07Z",
  "DefaultVersionId": "v6",
  "IsAttachable": true,
  "Path": "/",
  "PolicyId": "ANPAZKAPJZG4OUBBEQLZN",
  "PolicyName": "AmazonS3TablesFullAccess",
  "PolicyVersion": {
    "CreateDate": "2026-02-12T18:00:31Z",
    "Document": {
      "Statement": [
        {
          "Action": [
            "iam:PassRole"
          ],
          "Condition": {
            "StringEquals": {
              "iam:PassedToService": [
                "replication.s3tables.amazonaws.com"
              ]
            }
          },
          "Effect": "Allow",
          "Resource": "*",
          "Sid": "PassRoleToS3TablesReplication"
        },
        {
          "Action": [
            "s3tables:*"
          ],
          "Effect": "Allow",
          "Resource": "*"
        }
      ],
      "Version": "2012-10-17"
    },
    "IsDefaultVersion": true,
    "VersionId": "v6"
  },
  "UpdateDate": "2026-02-12T18:00:31Z"
}
