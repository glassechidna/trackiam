            "sso:GetSharedSsoConfiguration",
            "sso:ListDirectoryAssociations",
            "sso:ListProfileAssociations",
            "sso:ListProfiles"
          ],
          "Effect": "Allow",
          "Resource": "*",
          "Sid": "IAMIdentityCenterPermissions"
        }
      ],
      "Version": "2012-10-17"
    },
    "IsDefaultVersion": true,
