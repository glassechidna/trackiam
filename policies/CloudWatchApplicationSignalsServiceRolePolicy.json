{
  "Arn": "arn:aws:iam::aws:policy/aws-service-role/CloudWatchApplicationSignalsServiceRolePolicy",
  "CreateDate": "2023-11-09T18:09:57Z",
  "DefaultVersionId": "v4",
  "IsAttachable": true,
  "Path": "/aws-service-role/",
  "PolicyId": "ANPAZKAPJZG4LAFFLXUGT",
  "PolicyName": "CloudWatchApplicationSignalsServiceRolePolicy",
  "PolicyVersion": {
    "CreateDate": "2024-04-18T22:52:11Z",
    "Document": {
      "Statement": [
        {
          "Action": [
            "cloudwatch:ListMetrics"
          ],
          "Condition": {
            "StringEquals": {
              "aws:ResourceAccount": "${aws:PrincipalAccount}"
            }
          },
          "Effect": "Allow",
          "Resource": [
            "*"
          ],
          "Sid": "CWListMetricsPermission"
        },
        {
          "Action": [
            "logs:GetQueryResults",
            "logs:StartQuery"
          ],
          "Condition": {
            "StringEquals": {
              "aws:ResourceAccount": "${aws:PrincipalAccount}"
            }
          },
          "Effect": "Allow",
          "Resource": [
            "arn:aws:logs:*:*:log-group:/aws/application-signals/data:*",
            "arn:aws:logs:*:*:log-group:/aws/appsignals/*:*"
          ],
          "Sid": "CWLogsPermission"
        },
        {
          "Action": [
            "tag:GetResources"
          ],
          "Condition": {
            "StringEquals": {
              "aws:ResourceAccount": "${aws:PrincipalAccount}"
            }
          },
          "Effect": "Allow",
          "Resource": [
            "*"
          ],
          "Sid": "TagsPermission"
        },
        {
          "Action": [
            "xray:GetServiceGraph"
          ],
          "Condition": {
            "StringEquals": {
              "aws:ResourceAccount": "${aws:PrincipalAccount}"
            }
          },
          "Effect": "Allow",
          "Resource": [
            "*"
          ],
          "Sid": "XRayPermission"
        },
        {
          "Action": [
            "cloudwatch:GetMetricData"
          ],
          "Effect": "Allow",
          "Resource": [
            "*"
          ],
          "Sid": "CWGetMetricDataPermission"
        }
      ],
      "Version": "2012-10-17"
    },
    "IsDefaultVersion": true,
    "VersionId": "v4"
  },
  "UpdateDate": "2024-04-18T22:52:11Z"
}
