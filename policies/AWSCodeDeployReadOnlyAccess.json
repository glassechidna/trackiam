{
  "Arn": "arn:aws:iam::aws:policy/AWSCodeDeployReadOnlyAccess",
  "CreateDate": "2015-05-19T18:21:32Z",
  "DefaultVersionId": "v6",
  "IsAttachable": true,
  "Path": "/",
  "PolicyId": "ANPAILZHHKCKB4NE7XOIQ",
  "PolicyName": "AWSCodeDeployReadOnlyAccess",
  "PolicyVersion": {
    "CreateDate": "2026-02-12T17:57:13Z",
    "Document": {
      "Statement": [
        {
          "Action": [
            "codestar-notifications:DescribeNotificationRule"
          ],
          "Condition": {
            "ArnLike": {
              "codestar-notifications:NotificationsForResource": "arn:aws:codedeploy:*:*:application:*"
            }
          },
          "Effect": "Allow",
          "Resource": "*",
          "Sid": "CodeStarNotificationsPowerUserAccess"
        },
        {
          "Action": [
            "codedeploy:Batch*",
            "codedeploy:Get*",
            "codedeploy:List*"
          ],
          "Effect": "Allow",
          "Resource": "*"
        },
        {
          "Action": [
            "codestar-notifications:ListEventTypes",
            "codestar-notifications:ListNotificationRules",
            "codestar-notifications:ListTargets"
          ],
          "Effect": "Allow",
          "Resource": "*",
          "Sid": "CodeStarNotificationsListAccess"
        }
      ],
      "Version": "2012-10-17"
    },
    "IsDefaultVersion": true,
    "VersionId": "v6"
  },
  "UpdateDate": "2026-02-12T17:57:13Z"
}
