{
  "Arn": "arn:aws:iam::aws:policy/CloudFrontFullAccess",
  "CreateDate": "2015-02-06T18:39:50Z",
  "DefaultVersionId": "v10",
  "IsAttachable": true,
  "Path": "/",
  "PolicyId": "ANPAIPRV52SH6HDCCFY6U",
  "PolicyName": "CloudFrontFullAccess",
  "PolicyVersion": {
    "CreateDate": "2025-11-18T17:19:09Z",
    "Document": {
      "Statement": [
        {
          "Action": [
            "acm:RequestCertificate"
          ],
          "Condition": {
            "StringEquals": {
              "aws:CalledViaLast": "cloudfront.amazonaws.com"
            }
          },
          "Effect": "Allow",
          "Resource": "*",
          "Sid": "cfrequestcertificate"
        },
        {
          "Action": [
            "acm:DescribeCertificate",
            "acm:ListCertificates",
            "cloudfront-keyvaluestore:*",
            "cloudfront:*",
            "ec2:DescribeInstances",
            "ec2:DescribeInternetGateways",
            "ec2:DescribeIpamPools",
            "ec2:GetIpamPoolCidrs",
            "elasticloadbalancing:DescribeLoadBalancers",
            "iam:ListServerCertificates",
            "kinesis:ListStreams",
            "pricingplanmanager:CreateSubscription",
            "pricingplanmanager:ListSubscriptions",
            "waf:GetWebACL",
            "waf:ListWebACLs",
            "wafv2:CreateWebACL",
            "wafv2:GetWebACL",
            "wafv2:ListWebACLs"
          ],
          "Effect": "Allow",
          "Resource": "*",
          "Sid": "cffullaccess"
        },
        {
          "Action": [
            "iam:ListRoles"
          ],
          "Effect": "Allow",
          "Resource": "arn:aws:iam::*:*",
          "Sid": "cfflistroles"
        },
        {
          "Action": [
            "kinesis:DescribeStream"
          ],
          "Effect": "Allow",
          "Resource": "arn:aws:kinesis:*:*:*",
          "Sid": "cffdescribestream"
        },
        {
          "Action": [
            "pricingplanmanager:AssociateResourcesToSubscription",
            "pricingplanmanager:CancelSubscription",
            "pricingplanmanager:CancelSubscriptionChange",
            "pricingplanmanager:DisassociateResourcesFromSubscription",
            "pricingplanmanager:GetSubscription",
            "pricingplanmanager:UpdateSubscription"
          ],
          "Effect": "Allow",
          "Resource": "arn:aws:pricingplanmanager::*:subscription:*",
          "Sid": "ppmFullAccess"
        },
        {
          "Action": [
            "s3:ListAllMyBuckets"
          ],
          "Effect": "Allow",
          "Resource": "arn:aws:s3:::*",
          "Sid": "cfflistbuckets"
        }
      ],
      "Version": "2012-10-17"
    },
    "IsDefaultVersion": true,
    "VersionId": "v10"
  },
  "UpdateDate": "2025-11-18T17:19:09Z"
}
