{
  "Arn": "arn:aws:iam::aws:policy/aws-service-role/AWSBackupServiceLinkedRolePolicyForBackup",
  "CreateDate": "2020-06-02T23:08:40Z",
  "DefaultVersionId": "v12",
  "IsAttachable": true,
  "Path": "/aws-service-role/",
  "PolicyId": "ANPAZKAPJZG4ONJBD4ZY2",
  "PolicyName": "AWSBackupServiceLinkedRolePolicyForBackup",
  "PolicyVersion": {
    "CreateDate": "2022-11-10T22:51:20Z",
    "Document": {
      "Statement": [
        {
          "Action": "ec2:CreateTags",
          "Condition": {
            "ForAllValues:StringEquals": {
              "aws:TagKeys": [
                "AWSBackupManagedResource"
              ]
            }
          },
          "Effect": "Allow",
          "Resource": [
            "arn:aws:ec2:*::image/*",
            "arn:aws:ec2:*::snapshot/*"
          ]
        },
        {
          "Action": "ec2:CreateTags",
          "Condition": {
            "Null": {
              "ec2:ResourceTag/AWSBackupManagedResource": "false"
            }
          },
          "Effect": "Allow",
          "Resource": [
            "arn:aws:ec2:*::image/*",
            "arn:aws:ec2:*::snapshot/*"
          ]
        },
        {
          "Action": "ec2:CreateTags",
          "Condition": {
            "StringEquals": {
              "ec2:CreateAction": "CopySnapshot"
            }
          },
          "Effect": "Allow",
          "Resource": "arn:aws:ec2:*::snapshot/*"
        },
        {
          "Action": "kms:CreateGrant",
          "Condition": {
            "Bool": {
              "kms:GrantIsForAWSResource": "true"
            },
            "StringLike": {
              "kms:ViaService": [
                "ec2.*.amazonaws.com",
                "fsx.*.amazonaws.com",
                "rds.*.amazonaws.com"
              ]
            }
          },
          "Effect": "Allow",
          "Resource": "*"
        },
        {
          "Action": [
            "ec2:DeleteSnapshot",
            "ec2:DeregisterImage"
          ],
          "Condition": {
            "Null": {
              "ec2:ResourceTag/AWSBackupManagedResource": "false"
            }
          },
          "Effect": "Allow",
          "Resource": "*"
        },
        {
          "Action": [
            "elasticfilesystem:Backup",
            "elasticfilesystem:DescribeTags"
          ],
          "Condition": {
            "StringLike": {
              "aws:ResourceTag/aws:elasticfilesystem:default-backup": "enabled"
            }
          },
          "Effect": "Allow",
          "Resource": "arn:aws:elasticfilesystem:*:*:file-system/*"
        },
        {
          "Action": [
            "kms:GenerateDataKeyWithoutPlaintext",
            "kms:ListGrants",
            "kms:ReEncryptFrom"
          ],
          "Condition": {
            "StringLike": {
              "kms:ViaService": [
                "ec2.*.amazonaws.com",
                "fsx.*.amazonaws.com",
                "rds.*.amazonaws.com"
              ]
            }
          },
          "Effect": "Allow",
          "Resource": "*"
        },
        {
          "Action": "dynamodb:DeleteBackup",
          "Effect": "Allow",
          "Resource": "arn:aws:dynamodb:*:*:table/*/backup/*"
        },
        {
          "Action": "ec2:CopyImage",
          "Effect": "Allow",
          "Resource": "*"
        },
        {
          "Action": "ec2:CopySnapshot",
          "Effect": "Allow",
          "Resource": "arn:aws:ec2:*::snapshot/*"
        },
        {
          "Action": "events:ListRules",
          "Effect": "Allow",
          "Resource": "*"
        },
        {
          "Action": "kms:DescribeKey",
          "Effect": "Allow",
          "Resource": "*"
        },
        {
          "Action": [
            "dynamodb:DescribeTable",
            "dynamodb:ListTagsOfResource"
          ],
          "Effect": "Allow",
          "Resource": "arn:aws:dynamodb:*:*:table/*"
        },
        {
          "Action": [
            "dynamodb:ListTables",
            "ec2:DescribeInstances",
            "ec2:DescribeVolumes",
            "elasticfilesystem:DescribeFileSystems",
            "fsx:DescribeFileSystems",
            "fsx:DescribeVolumes",
            "rds:DescribeDBClusters",
            "rds:DescribeDBInstances",
            "s3:GetBucketTagging",
            "s3:ListAllMyBuckets",
            "storagegateway:ListVolumes",
            "tag:GetResources"
          ],
          "Effect": "Allow",
          "Resource": "*"
        },
        {
          "Action": [
            "ec2:DescribeImages",
            "ec2:DescribeSnapshots",
            "rds:DescribeDBClusterSnapshots",
            "rds:DescribeDBSnapshots"
          ],
          "Effect": "Allow",
          "Resource": "*"
        },
        {
          "Action": [
            "events:DeleteRule",
            "events:DescribeRule",
            "events:DisableRule",
            "events:EnableRule",
            "events:ListTargetsByRule",
            "events:PutRule",
            "events:PutTargets",
            "events:RemoveTargets"
          ],
          "Effect": "Allow",
          "Resource": [
            "arn:aws:events:*:*:rule/AwsBackupManagedRule*"
          ]
        },
        {
          "Action": [
            "fsx:CopyBackup",
            "fsx:DeleteBackup",
            "fsx:DescribeBackups",
            "fsx:TagResource"
          ],
          "Effect": "Allow",
          "Resource": "arn:aws:fsx:*:*:backup/*"
        },
        {
          "Action": [
            "rds:AddTagsToResource",
            "rds:CopyDBClusterSnapshot",
            "rds:DeleteDBClusterSnapshot"
          ],
          "Effect": "Allow",
          "Resource": "arn:aws:rds:*:*:cluster-snapshot:awsbackup:*"
        },
        {
          "Action": [
            "rds:AddTagsToResource",
            "rds:CopyDBSnapshot",
            "rds:DeleteDBSnapshot"
          ],
          "Effect": "Allow",
          "Resource": "arn:aws:rds:*:*:snapshot:awsbackup:*"
        },
        {
          "Action": [
            "redshift:DeleteClusterSnapshot"
          ],
          "Effect": "Allow",
          "Resource": [
            "arn:aws:redshift:*:*:snapshot:*/*"
          ]
        },
        {
          "Action": [
            "redshift:DescribeClusterSnapshots",
            "redshift:DescribeTags"
          ],
          "Effect": "Allow",
          "Resource": [
            "arn:aws:redshift:*:*:cluster:*",
            "arn:aws:redshift:*:*:snapshot:*/*"
          ]
        },
        {
          "Action": [
            "redshift:DescribeClusters"
          ],
          "Effect": "Allow",
          "Resource": [
            "arn:aws:redshift:*:*:cluster:*"
          ]
        },
        {
          "Action": [
            "ssm-sap:GetOperation",
            "sysops-sap:GetOperation"
          ],
          "Effect": "Allow",
          "Resource": "*"
        },
        {
          "Action": [
            "storagegateway:DescribeCachediSCSIVolumes",
            "storagegateway:DescribeStorediSCSIVolumes"
          ],
          "Effect": "Allow",
          "Resource": "arn:aws:storagegateway:*:*:gateway/*/volume/*"
        },
        {
          "Action": [
            "timestream:DescribeDatabase",
            "timestream:DescribeTable",
            "timestream:GetAwsBackupStatus",
            "timestream:GetAwsRestoreStatus",
            "timestream:ListDatabases",
            "timestream:ListTables",
            "timestream:ListTagsForResource"
          ],
          "Effect": "Allow",
          "Resource": [
            "arn:aws:timestream:*:*:database/*",
            "arn:aws:timestream:*:*:database/*/table/*"
          ]
        },
        {
          "Action": [
            "timestream:DescribeEndpoints"
          ],
          "Effect": "Allow",
          "Resource": "*"
        },
        {
          "Action": [
            "backup-gateway:ListTagsForResource"
          ],
          "Effect": "Allow",
          "Resource": "arn:aws:backup-gateway:*:*:vm/*",
          "Sid": "ListTagsForBackupGateway"
        },
        {
          "Action": [
            "backup-gateway:ListVirtualMachines"
          ],
          "Effect": "Allow",
          "Resource": "*",
          "Sid": "BackupGateway"
        }
      ],
      "Version": "2012-10-17"
    },
    "IsDefaultVersion": true,
    "VersionId": "v12"
  },
  "UpdateDate": "2022-11-10T22:51:20Z"
}
