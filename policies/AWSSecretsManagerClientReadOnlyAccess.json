{
  "Arn": "arn:aws:iam::aws:policy/AWSSecretsManagerClientReadOnlyAccess",
  "CreateDate": "2025-11-05T20:04:08Z",
  "DefaultVersionId": "v3",
  "IsAttachable": true,
  "Path": "/",
  "PolicyId": "ANPAZKAPJZG4NLTYLL76I",
  "PolicyName": "AWSSecretsManagerClientReadOnlyAccess",
  "PolicyVersion": {
    "CreateDate": "2026-02-12T18:00:42Z",
    "Document": {
      "Statement": [
        {
          "Action": [
            "kms:Decrypt"
          ],
          "Condition": {
            "StringLike": {
              "kms:EncryptionContext:SecretARN": "arn:aws:secretsmanager:*:*:secret:*",
              "kms:ViaService": "secretsmanager.*.amazonaws.com"
            }
          },
          "Effect": "Allow",
          "Resource": "arn:aws:kms:*:*:key/*",
          "Sid": "KMSDecryptKey"
        },
        {
          "Action": [
            "secretsmanager:DescribeSecret",
            "secretsmanager:GetSecretValue"
          ],
          "Effect": "Allow",
          "Resource": "arn:aws:secretsmanager:*:*:secret:*",
          "Sid": "SecretsManagerGetAndDescribeSecret"
        }
      ],
      "Version": "2012-10-17"
    },
    "IsDefaultVersion": true,
    "VersionId": "v3"
  },
  "UpdateDate": "2026-02-12T18:00:42Z"
}
