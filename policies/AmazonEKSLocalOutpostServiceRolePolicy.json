{
  "Arn": "arn:aws:iam::aws:policy/aws-service-role/AmazonEKSLocalOutpostServiceRolePolicy",
  "CreateDate": "2022-08-23T21:53:02Z",
  "DefaultVersionId": "v2",
  "IsAttachable": true,
  "Path": "/aws-service-role/",
  "PolicyId": "ANPAZKAPJZG4A6PGOGKJI",
  "PolicyName": "AmazonEKSLocalOutpostServiceRolePolicy",
  "PolicyVersion": {
    "CreateDate": "2022-10-24T16:24:51Z",
    "Document": {
      "Statement": [
        {
          "Action": "ec2:CreateTags",
          "Condition": {
            "ForAnyValue:StringLike": {
              "aws:TagKeys": [
                "eks*",
                "kubernetes.io/cluster/*"
              ]
            },
            "StringEquals": {
              "ec2:CreateAction": [
                "CreateNetworkInterface",
                "CreateSecurityGroup",
                "RunInstances"
              ]
            }
          },
          "Effect": "Allow",
          "Resource": [
            "arn:aws:ec2:*:*:instance/*",
            "arn:aws:ec2:*:*:network-interface/*",
            "arn:aws:ec2:*:*:security-group/*"
          ]
        },
        {
          "Action": "ec2:RunInstances",
          "Condition": {
            "StringLike": {
              "aws:RequestTag/eks-local:controlplane-name": "*"
            }
          },
          "Effect": "Allow",
          "Resource": "arn:aws:ec2:*:*:instance/*"
        },
        {
          "Action": "secretsmanager:DeleteSecret",
          "Condition": {
            "StringLike": {
              "aws:ResourceTag/eks-local:controlplane-name": "*"
            }
          },
          "Effect": "Allow",
          "Resource": "arn:aws:secretsmanager:*:*:secret:eks-local.cluster.x-k8s.io/*"
        },
        {
          "Action": [
            "ec2:AuthorizeSecurityGroupIngress",
            "ec2:DeleteNetworkInterface",
            "ec2:DeleteSecurityGroup",
            "ec2:GetConsoleOutput",
            "ec2:RevokeSecurityGroupIngress",
            "ec2:TerminateInstances"
          ],
          "Condition": {
            "StringLike": {
              "aws:ResourceTag/eks-local:controlplane-name": "*"
            }
          },
          "Effect": "Allow",
          "Resource": "*"
        },
        {
          "Action": [
            "ec2:CreateNetworkInterface"
          ],
          "Condition": {
            "StringLike": {
              "aws:RequestTag/eks-local:controlplane-name": "*"
            }
          },
          "Effect": "Allow",
          "Resource": "arn:aws:ec2:*:*:network-interface/*"
        },
        {
          "Action": [
            "ec2:CreateSecurityGroup"
          ],
          "Condition": {
            "StringLike": {
              "aws:RequestTag/eks-local:controlplane-name": "*"
            }
          },
          "Effect": "Allow",
          "Resource": "arn:aws:ec2:*:*:security-group/*"
        },
        {
          "Action": [
            "ec2:ModifyNetworkInterfaceAttribute"
          ],
          "Condition": {
            "StringLike": {
              "aws:ResourceTag/eks-local:controlplane-name": "*"
            }
          },
          "Effect": "Allow",
          "Resource": [
            "arn:aws:ec2:*:*:instance/*",
            "arn:aws:ec2:*:*:network-interface/*",
            "arn:aws:ec2:*:*:security-group/*"
          ]
        },
        {
          "Action": [
            "iam:PassRole"
          ],
          "Condition": {
            "StringEquals": {
              "iam:PassedToService": "ec2.amazonaws.com"
            }
          },
          "Effect": "Allow",
          "Resource": "*"
        },
        {
          "Action": [
            "secretsmanager:CreateSecret"
          ],
          "Condition": {
            "StringLike": {
              "aws:RequestTag/eks-local:controlplane-name": "*"
            }
          },
          "Effect": "Allow",
          "Resource": "arn:aws:secretsmanager:*:*:secret:eks-local.cluster.x-k8s.io/*"
        },
        {
          "Action": [
            "secretsmanager:TagResource"
          ],
          "Condition": {
            "ForAnyValue:StringLike": {
              "aws:TagKeys": [
                "eks*",
                "kubernetes.io/cluster/*"
              ]
            }
          },
          "Effect": "Allow",
          "Resource": "arn:aws:secretsmanager:*:*:secret:eks-local.cluster.x-k8s.io/*"
        },
        {
          "Action": [
            "ssm:StartSession"
          ],
          "Condition": {
            "StringLike": {
              "ssm:resourceTag/eks-local:controlplane-name": "*"
            }
          },
          "Effect": "Allow",
          "Resource": "arn:aws:ec2:*:*:instance/*"
        },
        {
          "Action": "ec2:RunInstances",
          "Effect": "Allow",
          "Resource": [
            "arn:aws:ec2:*:*:image/*",
            "arn:aws:ec2:*:*:launch-template/*",
            "arn:aws:ec2:*:*:network-interface/*",
            "arn:aws:ec2:*:*:placement-group/*",
            "arn:aws:ec2:*:*:security-group/*",
            "arn:aws:ec2:*:*:subnet/*",
            "arn:aws:ec2:*:*:volume/*"
          ]
        },
        {
          "Action": "secretsmanager:DescribeSecret",
          "Effect": "Allow",
          "Resource": "arn:aws:secretsmanager:*:*:secret:eks-local.cluster.x-k8s.io/*"
        },
        {
          "Action": [
            "ec2:CreateNetworkInterface"
          ],
          "Effect": "Allow",
          "Resource": [
            "arn:aws:ec2:*:*:security-group/*",
            "arn:aws:ec2:*:*:subnet/*"
          ]
        },
        {
          "Action": [
            "ec2:CreateSecurityGroup"
          ],
          "Effect": "Allow",
          "Resource": "arn:aws:ec2:*:*:vpc/*"
        },
        {
          "Action": [
            "ec2:DescribeAddresses",
            "ec2:DescribeImages",
            "ec2:DescribeInstances",
            "ec2:DescribeNetworkInterfaceAttribute",
            "ec2:DescribeNetworkInterfaces",
            "ec2:DescribePlacementGroups",
            "ec2:DescribeRouteTables",
            "ec2:DescribeSecurityGroups",
            "ec2:DescribeSubnets",
            "ec2:DescribeVpcAttribute",
            "ec2:DescribeVpcs"
          ],
          "Effect": "Allow",
          "Resource": "*"
        },
        {
          "Action": [
            "iam:DeleteInstanceProfile",
            "iam:GetInstanceProfile",
            "iam:RemoveRoleFromInstanceProfile"
          ],
          "Effect": "Allow",
          "Resource": "arn:aws:iam::*:instance-profile/eks-local-*"
        },
        {
          "Action": [
            "outposts:GetOutpost"
          ],
          "Effect": "Allow",
          "Resource": "*"
        },
        {
          "Action": [
            "ssm:ResumeSession",
            "ssm:TerminateSession"
          ],
          "Effect": "Allow",
          "Resource": "*"
        },
        {
          "Action": [
            "ssm:StartSession"
          ],
          "Effect": "Allow",
          "Resource": "arn:aws:ssm:*::document/AmazonEKS-ControlPlaneInstanceProxy"
        }
      ],
      "Version": "2012-10-17"
    },
    "IsDefaultVersion": true,
    "VersionId": "v2"
  },
  "UpdateDate": "2022-10-24T16:24:51Z"
}
