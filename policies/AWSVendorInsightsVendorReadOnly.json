{
  "Arn": "arn:aws:iam::aws:policy/AWSVendorInsightsVendorReadOnly",
  "CreateDate": "2022-07-26T15:05:34Z",
  "DefaultVersionId": "v2",
  "IsAttachable": true,
  "Path": "/",
  "PolicyId": "ANPAZKAPJZG4NHHAIIJR5",
  "PolicyName": "AWSVendorInsightsVendorReadOnly",
  "PolicyVersion": {
    "CreateDate": "2022-12-01T00:54:38Z",
    "Document": {
      "Statement": [
        {
          "Action": "aws-marketplace:DescribeEntity",
          "Effect": "Allow",
          "Resource": "arn:aws:aws-marketplace:*:*:*/SaaSProduct/*"
        },
        {
          "Action": "aws-marketplace:ListEntities",
          "Effect": "Allow",
          "Resource": "*"
        },
        {
          "Action": [
            "artifact:GetReport",
            "artifact:GetReportMetadata",
            "artifact:GetTermForReport",
            "artifact:ListReports"
          ],
          "Effect": "Allow",
          "Resource": "arn:aws:artifact:*::report/*"
        },
        {
          "Action": [
            "vendor-insights:GetDataSource",
            "vendor-insights:GetSecurityProfile",
            "vendor-insights:GetSecurityProfileSnapshot",
            "vendor-insights:ListDataSources",
            "vendor-insights:ListSecurityProfileSnapshots",
            "vendor-insights:ListSecurityProfiles",
            "vendor-insights:ListTagsForResource"
          ],
          "Effect": "Allow",
          "Resource": "*"
        }
      ],
      "Version": "2012-10-17"
    },
    "IsDefaultVersion": true,
    "VersionId": "v2"
  },
  "UpdateDate": "2022-12-01T00:54:38Z"
}
