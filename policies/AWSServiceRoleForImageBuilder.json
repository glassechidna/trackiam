{
  "Arn": "arn:aws:iam::aws:policy/aws-service-role/AWSServiceRoleForImageBuilder",
  "CreateDate": "2019-11-29T22:02:13Z",
  "DefaultVersionId": "v26",
  "IsAttachable": true,
  "Path": "/aws-service-role/",
  "PolicyId": "ANPAZKAPJZG4NE22WISEW",
  "PolicyName": "AWSServiceRoleForImageBuilder",
  "PolicyVersion": {
    "CreateDate": "2026-02-23T22:27:06Z",
    "Document": {
      "Statement": [
        {
          "Action": "ec2:RegisterImage",
          "Condition": {
            "StringEquals": {
              "aws:RequestTag/CreatedBy": "EC2 Image Builder"
            }
          },
          "Effect": "Allow",
          "Resource": [
            "arn:aws:ec2:*::image/*"
          ]
        },
        {
          "Action": "ec2:RegisterImage",
          "Condition": {
            "StringEquals": {
              "ec2:ResourceTag/CreatedBy": "EC2 Image Builder"
            }
          },
          "Effect": "Allow",
          "Resource": [
            "arn:aws:ec2:*::snapshot/*"
          ]
        },
        {
          "Action": "iam:CreateServiceLinkedRole",
          "Condition": {
            "StringEquals": {
              "iam:AWSServiceName": [
                "ec2fastlaunch.amazonaws.com",
                "ssm.amazonaws.com"
              ]
            }
          },
          "Effect": "Allow",
          "Resource": "*"
        },
        {
          "Action": "iam:PassRole",
          "Condition": {
            "StringEquals": {
              "iam:PassedToService": [
                "ec2.amazonaws.com",
                "ec2.amazonaws.com.cn",
                "vmie.amazonaws.com"
              ]
            }
          },
          "Effect": "Allow",
          "Resource": "*"
        },
        {
          "Action": "kms:CreateGrant",
          "Condition": {
            "Bool": {
              "kms:GrantIsForAWSResource": true
            },
            "StringLike": {
              "kms:ViaService": [
                "ec2.*.amazonaws.com"
              ]
            }
          },
          "Effect": "Allow",
          "Resource": "*"
        },
        {
          "Action": [
            "ec2:CancelExportTask"
          ],
          "Condition": {
            "StringEquals": {
              "ec2:ResourceTag/CreatedBy": "EC2 Image Builder"
            }
          },
          "Effect": "Allow",
          "Resource": "arn:aws:ec2:*:*:export-image-task/*"
        },
        {
          "Action": [
            "ec2:CreateTags"
          ],
          "Condition": {
            "StringEquals": {
              "aws:RequestTag/CreatedBy": [
                "EC2 Fast Launch",
                "EC2 Image Builder"
              ]
            }
          },
          "Effect": "Allow",
          "Resource": [
            "arn:aws:ec2:*:*:launch-template/*",
            "arn:aws:ec2:*::snapshot/*"
          ]
        },
        {
          "Action": [
            "ec2:CreateTags"
          ],
          "Condition": {
            "StringEquals": {
              "aws:RequestTag/CreatedBy": [
                "EC2 Fast Launch",
                "EC2 Image Builder"
              ],
              "ec2:CreateAction": [
                "CreateImage",
                "RunInstances"
              ]
            }
          },
          "Effect": "Allow",
          "Resource": "*"
        },
        {
          "Action": [
            "ec2:EnableFastLaunch"
          ],
          "Condition": {
            "StringEquals": {
              "ec2:ResourceTag/CreatedBy": "EC2 Image Builder"
            }
          },
          "Effect": "Allow",
          "Resource": [
            "arn:aws:ec2:*:*:launch-template/*",
            "arn:aws:ec2:*::image/*"
          ]
        },
        {
          "Action": [
            "ec2:ExportImage"
          ],
          "Condition": {
            "StringEquals": {
              "ec2:ResourceTag/CreatedBy": "EC2 Image Builder"
            }
          },
          "Effect": "Allow",
          "Resource": "arn:aws:ec2:*::image/*"
        },
        {
          "Action": [
            "ec2:ModifySnapshotAttribute"
          ],
          "Condition": {
            "StringEquals": {
              "ec2:ResourceTag/CreatedBy": "EC2 Image Builder"
            }
          },
          "Effect": "Allow",
          "Resource": "arn:aws:ec2:*::snapshot/*"
        },
        {
          "Action": [
            "ec2:RunInstances"
          ],
          "Condition": {
            "StringEquals": {
              "aws:RequestTag/CreatedBy": [
                "EC2 Fast Launch",
                "EC2 Image Builder"
              ]
            }
          },
          "Effect": "Allow",
          "Resource": [
            "arn:aws:ec2:*:*:instance/*",
            "arn:aws:ec2:*:*:volume/*"
          ]
        },
        {
          "Action": [
            "ec2:StartInstances",
            "ec2:StopInstances",
            "ec2:TerminateInstances"
          ],
          "Condition": {
            "StringEquals": {
              "ec2:ResourceTag/CreatedBy": "EC2 Image Builder"
            }
          },
          "Effect": "Allow",
          "Resource": "*"
        },
        {
          "Action": [
            "ecr:BatchDeleteImage"
          ],
          "Condition": {
            "StringEquals": {
              "ecr:ResourceTag/CreatedBy": "EC2 Image Builder"
            }
          },
          "Effect": "Allow",
          "Resource": "arn:aws:ecr:*:*:repository/image-builder-*"
        },
        {
          "Action": [
            "ecr:CreateRepository"
          ],
          "Condition": {
            "StringEquals": {
              "aws:RequestTag/CreatedBy": "EC2 Image Builder"
            }
          },
          "Effect": "Allow",
          "Resource": "*"
        },
        {
          "Action": [
            "ecr:TagResource"
          ],
          "Condition": {
            "StringEquals": {
              "aws:RequestTag/CreatedBy": "EC2 Image Builder"
            }
          },
          "Effect": "Allow",
          "Resource": "arn:aws:ecr:*:*:repository/image-builder-*"
        },
        {
          "Action": [
            "kms:Decrypt",
            "kms:Encrypt",
            "kms:GenerateDataKeyWithoutPlaintext",
            "kms:ReEncryptFrom",
            "kms:ReEncryptTo"
          ],
          "Condition": {
            "ForAllValues:StringEquals": {
              "kms:EncryptionContextKeys": [
                "aws:ebs:id"
              ]
            },
            "StringLike": {
              "kms:ViaService": [
                "ec2.*.amazonaws.com"
              ]
            }
          },
          "Effect": "Allow",
          "Resource": "*"
        },
        {
          "Action": [
            "kms:DescribeKey"
          ],
          "Condition": {
            "StringLike": {
              "kms:ViaService": [
                "ec2.*.amazonaws.com"
              ]
            }
          },
          "Effect": "Allow",
          "Resource": "*"
        },
        {
          "Action": [
            "ssm:SendCommand"
          ],
          "Condition": {
            "StringEquals": {
              "ssm:resourceTag/CreatedBy": [
                "EC2 Image Builder"
              ]
            }
          },
          "Effect": "Allow",
          "Resource": [
            "arn:aws:ec2:*:*:instance/*"
          ]
        },
        {
          "Action": "ssm:SendCommand",
          "Effect": "Allow",
          "Resource": [
            "arn:aws:s3:::*",
            "arn:aws:ssm:*:*:document/AWS-RunPowerShellScript",
            "arn:aws:ssm:*:*:document/AWS-RunShellScript",
            "arn:aws:ssm:*:*:document/AWSEC2-RunSysprep"
          ]
        },
        {
          "Action": "sts:AssumeRole",
          "Effect": "Allow",
          "Resource": "arn:aws:iam::*:role/EC2ImageBuilderDistributionCrossAccountRole"
        },
        {
          "Action": [
            "ec2:CopyImage",
            "ec2:CreateImage",
            "ec2:CreateLaunchTemplate",
            "ec2:DeregisterImage",
            "ec2:DescribeExportImageTasks",
            "ec2:DescribeHosts",
            "ec2:DescribeImages",
            "ec2:DescribeImportImageTasks",
            "ec2:DescribeInstanceAttribute",
            "ec2:DescribeInstanceStatus",
            "ec2:DescribeInstanceTypeOfferings",
            "ec2:DescribeInstanceTypes",
            "ec2:DescribeInstances",
            "ec2:DescribeSnapshots",
            "ec2:DescribeSubnets",
            "ec2:DescribeTags",
            "ec2:ModifyImageAttribute"
          ],
          "Effect": "Allow",
          "Resource": "*"
        },
        {
          "Action": [
            "ec2:CreateLaunchTemplateVersion",
            "ec2:DescribeLaunchTemplateVersions",
            "ec2:DescribeLaunchTemplates",
            "ec2:ModifyLaunchTemplate"
          ],
          "Effect": "Allow",
          "Resource": "*"
        },
        {
          "Action": [
            "ec2:CreateTags"
          ],
          "Effect": "Allow",
          "Resource": [
            "arn:aws:ec2:*:*:export-image-task/*",
            "arn:aws:ec2:*::image/*"
          ]
        },
        {
          "Action": [
            "ec2:ExportImage"
          ],
          "Effect": "Allow",
          "Resource": "arn:aws:ec2:*:*:export-image-task/*"
        },
        {
          "Action": [
            "ec2:RunInstances"
          ],
          "Effect": "Allow",
          "Resource": [
            "arn:aws:ec2:*:*:key-pair/*",
            "arn:aws:ec2:*:*:launch-template/*",
            "arn:aws:ec2:*:*:network-interface/*",
            "arn:aws:ec2:*:*:security-group/*",
            "arn:aws:ec2:*:*:subnet/*",
            "arn:aws:ec2:*::image/*",
            "arn:aws:ec2:*::snapshot/*",
            "arn:aws:license-manager:*:*:license-configuration:*"
          ]
        },
        {
          "Action": [
            "events:DeleteRule",
            "events:DescribeRule",
            "events:PutRule",
            "events:PutTargets",
            "events:RemoveTargets"
          ],
          "Effect": "Allow",
          "Resource": [
            "arn:aws:events:*:*:rule/ImageBuilder-*"
          ]
        },
        {
          "Action": [
            "inspector2:ListCoverage",
            "inspector2:ListFindings"
          ],
          "Effect": "Allow",
          "Resource": "*"
        },
        {
          "Action": [
            "license-manager:UpdateLicenseSpecificationsForResource"
          ],
          "Effect": "Allow",
          "Resource": "*"
        },
        {
          "Action": [
            "logs:CreateLogGroup",
            "logs:CreateLogStream",
            "logs:PutLogEvents"
          ],
          "Effect": "Allow",
          "Resource": "arn:aws:logs:*:*:log-group:/aws/imagebuilder/*"
        },
        {
          "Action": [
            "sns:Publish"
          ],
          "Effect": "Allow",
          "Resource": "*"
        },
        {
          "Action": [
            "ssm:AddTagsToResource",
            "ssm:DescribeAssociationExecutions",
            "ssm:DescribeInstanceAssociationsStatus",
            "ssm:DescribeInstanceInformation",
            "ssm:GetAutomationExecution",
            "ssm:GetCommandInvocation",
            "ssm:ListCommandInvocations",
            "ssm:ListCommands",
            "ssm:ListInventoryEntries",
            "ssm:SendAutomationSignal",
            "ssm:StopAutomationExecution"
          ],
          "Effect": "Allow",
          "Resource": "*"
        },
        {
          "Action": [
            "ssm:CreateAssociation",
            "ssm:DeleteAssociation"
          ],
          "Effect": "Allow",
          "Resource": [
            "arn:aws:ec2:*:*:instance/*",
            "arn:aws:ssm:*:*:association/*",
            "arn:aws:ssm:*:*:document/AWS-GatherSoftwareInventory"
          ]
        },
        {
          "Action": [
            "ssm:GetParameter"
          ],
          "Effect": "Allow",
          "Resource": "arn:aws:ssm:*::parameter/aws/service/*"
        },
        {
          "Action": [
            "ssm:GetParameter",
            "ssm:PutParameter"
          ],
          "Effect": "Allow",
          "Resource": "arn:aws:ssm:*:*:parameter/imagebuilder/*"
        }
      ],
      "Version": "2012-10-17"
    },
    "IsDefaultVersion": true,
    "VersionId": "v26"
  },
  "UpdateDate": "2026-02-23T22:27:06Z"
}
