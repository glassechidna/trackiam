{
  "Arn": "arn:aws:iam::aws:policy/AmazonWorkSpacesThinClientReadOnlyAccess",
  "CreateDate": "2024-07-19T08:50:52Z",
  "DefaultVersionId": "v3",
  "IsAttachable": true,
  "Path": "/",
  "PolicyId": "ANPAZKAPJZG4C45JOKZDB",
  "PolicyName": "AmazonWorkSpacesThinClientReadOnlyAccess",
  "PolicyVersion": {
    "CreateDate": "2025-01-09T16:52:05Z",
    "Document": {
      "Statement": [
        {
          "Action": [
            "appstream:DescribeStacks"
          ],
          "Effect": "Allow",
          "Resource": "*",
          "Sid": "AllowAppStreamAccess"
        },
        {
          "Action": [
            "thinclient:GetDevice",
            "thinclient:GetDeviceDetails",
            "thinclient:GetEnvironment",
            "thinclient:GetSoftwareSet",
            "thinclient:ListDeviceSessions",
            "thinclient:ListDevices",
            "thinclient:ListEnvironments",
            "thinclient:ListSoftwareSets",
            "thinclient:ListTagsForResource"
          ],
          "Effect": "Allow",
          "Resource": "*",
          "Sid": "AllowThinClientReadAccess"
        },
        {
          "Action": [
            "workspaces-web:GetPortal",
            "workspaces-web:GetUserSettings",
            "workspaces-web:ListPortals"
          ],
          "Effect": "Allow",
          "Resource": "*",
          "Sid": "AllowWorkSpacesSecureBrowserAccess"
        },
        {
          "Action": [
            "workspaces:DescribeConnectionAliases",
            "workspaces:DescribeWorkspaceDirectories"
          ],
          "Effect": "Allow",
          "Resource": "*",
          "Sid": "AllowWorkSpacesAccess"
        }
      ],
      "Version": "2012-10-17"
    },
    "IsDefaultVersion": true,
    "VersionId": "v3"
  },
  "UpdateDate": "2025-01-09T16:52:05Z"
}
