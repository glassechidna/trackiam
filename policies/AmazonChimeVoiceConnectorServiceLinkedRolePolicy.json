{
  "Arn": "arn:aws:iam::aws:policy/aws-service-role/AmazonChimeVoiceConnectorServiceLinkedRolePolicy",
  "CreateDate": "2019-09-30T22:16:42Z",
  "DefaultVersionId": "v4",
  "IsAttachable": true,
  "Path": "/aws-service-role/",
  "PolicyId": "ANPAZKAPJZG4GP44ZBY4P",
  "PolicyName": "AmazonChimeVoiceConnectorServiceLinkedRolePolicy",
  "PolicyVersion": {
    "CreateDate": "2023-03-23T19:39:40Z",
    "Document": {
      "Statement": [
        {
          "Action": [
            "SNS:Publish"
          ],
          "Effect": "Allow",
          "Resource": [
            "arn:aws:sns:*:*:ChimeVoiceConnector-Streaming*"
          ]
        },
        {
          "Action": [
            "chime:CreateMediaInsightsPipeline"
          ],
          "Effect": "Allow",
          "Resource": [
            "*"
          ]
        },
        {
          "Action": [
            "chime:GetVoiceConnector*"
          ],
          "Effect": "Allow",
          "Resource": [
            "*"
          ]
        },
        {
          "Action": [
            "kinesisvideo:CreateStream",
            "kinesisvideo:DescribeStream",
            "kinesisvideo:GetDataEndpoint",
            "kinesisvideo:PutMedia",
            "kinesisvideo:UpdateDataRetention"
          ],
          "Effect": "Allow",
          "Resource": [
            "arn:aws:kinesisvideo:*:*:stream/ChimeVoiceConnector-*"
          ]
        },
        {
          "Action": [
            "kinesisvideo:ListStreams"
          ],
          "Effect": "Allow",
          "Resource": [
            "*"
          ]
        },
        {
          "Action": [
            "polly:SynthesizeSpeech"
          ],
          "Effect": "Allow",
          "Resource": [
            "*"
          ]
        },
        {
          "Action": [
            "sqs:SendMessage"
          ],
          "Effect": "Allow",
          "Resource": [
            "arn:aws:sqs:*:*:ChimeVoiceConnector-Streaming*"
          ]
        }
      ],
      "Version": "2012-10-17"
    },
    "IsDefaultVersion": true,
    "VersionId": "v4"
  },
  "UpdateDate": "2023-03-23T19:39:40Z"
}
