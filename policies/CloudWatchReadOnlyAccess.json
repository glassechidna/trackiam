{
  "Arn": "arn:aws:iam::aws:policy/CloudWatchReadOnlyAccess",
  "CreateDate": "2015-02-06T18:40:01Z",
  "DefaultVersionId": "v5",
  "IsAttachable": true,
  "Path": "/",
  "PolicyId": "ANPAJN23PDQP7SZQAE3QE",
  "PolicyName": "CloudWatchReadOnlyAccess",
  "PolicyVersion": {
    "CreateDate": "2022-11-27T13:08:46Z",
    "Document": {
      "Statement": [
        {
          "Action": [
            "autoscaling:Describe*",
            "cloudwatch:Describe*",
            "cloudwatch:Get*",
            "cloudwatch:List*",
            "logs:Describe*",
            "logs:FilterLogEvents",
            "logs:Get*",
            "logs:List*",
            "logs:StartQuery",
            "logs:StopQuery",
            "logs:TestMetricFilter",
            "oam:ListSinks",
            "sns:Get*",
            "sns:List*"
          ],
          "Effect": "Allow",
          "Resource": "*"
        },
        {
          "Action": [
            "oam:ListAttachedLinks"
          ],
          "Effect": "Allow",
          "Resource": "arn:aws:oam:*:*:sink/*"
        }
      ],
      "Version": "2012-10-17"
    },
    "IsDefaultVersion": true,
    "VersionId": "v5"
  },
  "UpdateDate": "2022-11-27T13:08:46Z"
