{
  "Arn": "arn:aws:iam::aws:policy/CloudWatchReadOnlyAccess",
  "CreateDate": "2015-02-06T18:40:01Z",
  "DefaultVersionId": "v9",
  "IsAttachable": true,
  "Path": "/",
  "PolicyId": "ANPAJN23PDQP7SZQAE3QE",
  "PolicyName": "CloudWatchReadOnlyAccess",
  "PolicyVersion": {
    "CreateDate": "2024-05-17T22:17:51Z",
    "Document": {
      "Statement": [
        {
          "Action": "iam:GetRole",
          "Effect": "Allow",
          "Resource": "arn:aws:iam::*:role/aws-service-role/application-signals.cloudwatch.amazonaws.com/AWSServiceRoleForCloudWatchApplicationSignals",
          "Sid": "CloudWatchReadOnlyGetRolePermissions"
        },
        {
          "Action": [
            "application-autoscaling:DescribeScalingPolicies",
            "application-signals:BatchGet*",
            "application-signals:Get*",
            "application-signals:List*",
            "autoscaling:Describe*",
            "cloudwatch:BatchGet*",
            "cloudwatch:Describe*",
            "cloudwatch:GenerateQuery",
            "cloudwatch:Get*",
            "cloudwatch:List*",
            "logs:Describe*",
            "logs:FilterLogEvents",
            "logs:Get*",
            "logs:List*",
            "logs:StartLiveTail",
            "logs:StartQuery",
            "logs:StopLiveTail",
            "logs:StopQuery",
            "logs:TestMetricFilter",
            "oam:ListSinks",
            "rum:BatchGet*",
            "rum:Get*",
            "rum:List*",
            "sns:Get*",
            "sns:List*",
            "synthetics:Describe*",
            "synthetics:Get*",
            "synthetics:List*",
            "xray:BatchGet*",
            "xray:Get*"
          ],
          "Effect": "Allow",
          "Resource": "*",
          "Sid": "CloudWatchReadOnlyAccessPermissions"
        },
        {
          "Action": [
            "oam:ListAttachedLinks"
          ],
          "Effect": "Allow",
          "Resource": "arn:aws:oam:*:*:sink/*",
          "Sid": "OAMReadPermissions"
        }
      ],
      "Version": "2012-10-17"
    },
    "IsDefaultVersion": true,
    "VersionId": "v9"
  },
  "UpdateDate": "2024-05-17T22:17:51Z"
}
