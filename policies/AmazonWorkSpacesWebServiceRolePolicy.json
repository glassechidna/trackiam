{
  "Arn": "arn:aws:iam::aws:policy/aws-service-role/AmazonWorkSpacesWebServiceRolePolicy",
  "CreateDate": "2021-11-30T13:15:53Z",
  "DefaultVersionId": "v4",
  "IsAttachable": true,
  "Path": "/aws-service-role/",
  "PolicyId": "ANPAZKAPJZG4JW4G4DATC",
  "PolicyName": "AmazonWorkSpacesWebServiceRolePolicy",
  "PolicyVersion": {
    "CreateDate": "2022-10-17T16:43:45Z",
    "Document": {
      "Statement": [
        {
          "Action": [
            "cloudwatch:PutMetricData"
          ],
          "Condition": {
            "StringEquals": {
              "cloudwatch:namespace": [
                "AWS/Usage",
                "AWS/WorkSpacesWeb"
              ]
            }
          },
          "Effect": "Allow",
          "Resource": "*"
        },
        {
          "Action": [
            "ec2:CreateTags"
          ],
          "Condition": {
            "ForAllValues:StringEquals": {
              "aws:TagKeys": [
                "WorkSpacesWebManaged"
              ]
            },
            "StringEquals": {
              "ec2:CreateAction": "CreateNetworkInterface"
            }
          },
          "Effect": "Allow",
          "Resource": "arn:aws:ec2:*:*:network-interface/*"
        },
        {
          "Action": [
            "ec2:AssociateAddress",
            "ec2:CreateNetworkInterface",
            "ec2:DeleteNetworkInterface",
            "ec2:DescribeAvailabilityZones",
            "ec2:DescribeNetworkInterfaces",
            "ec2:DescribeRouteTables",
            "ec2:DescribeSecurityGroups",
            "ec2:DescribeSubnets",
            "ec2:DescribeVpcEndpoints",
            "ec2:DescribeVpcs",
            "ec2:DisassociateAddress"
          ],
          "Effect": "Allow",
          "Resource": "*"
        },
        {
          "Action": [
            "kinesis:DescribeStreamSummary",
            "kinesis:PutRecord",
            "kinesis:PutRecords"
          ],
          "Effect": "Allow",
          "Resource": "arn:aws:kinesis:*:*:stream/amazon-workspaces-web-*"
        }
      ],
      "Version": "2012-10-17"
    },
    "IsDefaultVersion": true,
    "VersionId": "v4"
  },
  "UpdateDate": "2022-10-17T16:43:45Z"
}
