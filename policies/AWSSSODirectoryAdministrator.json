{
  "Arn": "arn:aws:iam::aws:policy/AWSSSODirectoryAdministrator",
  "CreateDate": "2018-10-31T23:54:00Z",
  "DefaultVersionId": "v5",
  "IsAttachable": true,
  "Path": "/",
  "PolicyId": "ANPAI2TCZRD7WRD5D2E2Q",
  "PolicyName": "AWSSSODirectoryAdministrator",
  "PolicyVersion": {
    "CreateDate": "2025-09-16T19:49:08Z",
    "Document": {
      "Statement": [
        {
          "Action": [
            "kms:Decrypt"
          ],
          "Condition": {
            "StringLike": {
              "kms:EncryptionContext:aws:identitystore:identitystore-arn": "*",
              "kms:ViaService": "identitystore.*.amazonaws.com"
            }
          },
          "Effect": "Allow",
          "Resource": "*",
          "Sid": "AllowKMSKeyUseViaAWSIdentityStoreService"
        },
        {
          "Action": [
            "identitystore-auth:*",
            "identitystore:*",
            "sso-directory:*",
            "sso:ListDirectoryAssociations"
          ],
          "Effect": "Allow",
          "Resource": "*",
          "Sid": "AWSSSODirectoryAdministrator"
        }
      ],
      "Version": "2012-10-17"
    },
    "IsDefaultVersion": true,
    "VersionId": "v5"
  },
  "UpdateDate": "2025-09-16T19:49:08Z"
}
