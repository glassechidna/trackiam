{
  "Arn": "arn:aws:iam::aws:policy/aws-service-role/AWSApplicationMigrationServiceRolePolicy",
  "CreateDate": "2021-04-07T06:43:20Z",
  "DefaultVersionId": "v1",
  "IsAttachable": true,
  "Path": "/aws-service-role/",
  "PolicyId": "ANPAZKAPJZG4LGJRHTEPG",
  "PolicyName": "AWSApplicationMigrationServiceRolePolicy",
  "PolicyVersion": {
    "CreateDate": "2021-04-07T06:43:20Z",
    "Document": {
      "Statement": [
        {
          "Action": "ec2:CreateTags",
          "Condition": {
            "StringEquals": {
              "ec2:CreateAction": [
                "CreateLaunchTemplate",
                "CreateSecurityGroup",
                "CreateSnapshot",
                "CreateVolume",
                "RunInstances"
              ]
            }
          },
          "Effect": "Allow",
          "Resource": [
            "arn:aws:ec2:*:*:instance/*",
            "arn:aws:ec2:*:*:launch-template/*",
            "arn:aws:ec2:*:*:security-group/*",
            "arn:aws:ec2:*:*:snapshot/*",
            "arn:aws:ec2:*:*:volume/*"
          ]
        },
        {
          "Action": "iam:PassRole",
          "Condition": {
            "StringEquals": {
              "iam:PassedToService": "ec2.amazonaws.com"
            }
          },
          "Effect": "Allow",
          "Resource": [
            "arn:aws:iam::*:role/service-role/AWSApplicationMigrationConversionServerRole",
            "arn:aws:iam::*:role/service-role/AWSApplicationMigrationReplicationServerRole"
          ]
        },
        {
          "Action": [
            "ec2:AttachVolume"
          ],
          "Condition": {
            "Null": {
              "ec2:ResourceTag/AWSApplicationMigrationServiceManaged": "false"
            }
          },
          "Effect": "Allow",
          "Resource": "arn:aws:ec2:*:*:volume/*"
        },
        {
          "Action": [
            "ec2:AttachVolume",
            "ec2:DetachVolume"
          ],
          "Condition": {
            "Null": {
              "ec2:ResourceTag/AWSApplicationMigrationServiceManaged": "false"
            }
          },
          "Effect": "Allow",
          "Resource": "arn:aws:ec2:*:*:instance/*"
        },
        {
          "Action": [
            "ec2:AuthorizeSecurityGroupEgress",
            "ec2:AuthorizeSecurityGroupIngress",
            "ec2:RevokeSecurityGroupEgress"
          ],
          "Condition": {
            "Null": {
              "aws:ResourceTag/AWSApplicationMigrationServiceManaged": "false"
            }
          },
          "Effect": "Allow",
          "Resource": "arn:aws:ec2:*:*:security-group/*"
        },
        {
          "Action": [
            "ec2:CreateLaunchTemplate"
          ],
          "Condition": {
            "Null": {
              "aws:RequestTag/AWSApplicationMigrationServiceManaged": "false"
            }
          },
          "Effect": "Allow",
          "Resource": "arn:aws:ec2:*:*:launch-template/*"
        },
        {
          "Action": [
            "ec2:CreateLaunchTemplateVersion",
            "ec2:DeleteLaunchTemplate",
            "ec2:DeleteLaunchTemplateVersions",
            "ec2:ModifyLaunchTemplate"
          ],
          "Condition": {
            "Null": {
              "aws:ResourceTag/AWSApplicationMigrationServiceManaged": "false"
            }
          },
          "Effect": "Allow",
          "Resource": "arn:aws:ec2:*:*:launch-template/*"
        },
        {
          "Action": [
            "ec2:CreateSecurityGroup"
          ],
          "Condition": {
            "Null": {
              "aws:RequestTag/AWSApplicationMigrationServiceManaged": "false"
            }
          },
          "Effect": "Allow",
          "Resource": "arn:aws:ec2:*:*:security-group/*"
        },
        {
          "Action": [
            "ec2:CreateSnapshot"
          ],
          "Condition": {
            "Null": {
              "aws:RequestTag/AWSApplicationMigrationServiceManaged": "false"
            }
          },
          "Effect": "Allow",
          "Resource": "arn:aws:ec2:*:*:snapshot/*"
        },
        {
          "Action": [
            "ec2:CreateSnapshot"
          ],
          "Condition": {
            "Null": {
              "ec2:ResourceTag/AWSApplicationMigrationServiceManaged": "false"
            }
          },
          "Effect": "Allow",
          "Resource": "arn:aws:ec2:*:*:volume/*"
        },
        {
          "Action": [
            "ec2:CreateVolume"
          ],
          "Condition": {
            "Null": {
              "aws:RequestTag/AWSApplicationMigrationServiceManaged": "false"
            }
          },
          "Effect": "Allow",
          "Resource": "arn:aws:ec2:*:*:volume/*"
        },
        {
          "Action": [
            "ec2:DeleteSnapshot"
          ],
          "Condition": {
            "Null": {
              "aws:ResourceTag/AWSApplicationMigrationServiceManaged": "false"
            }
          },
          "Effect": "Allow",
          "Resource": "arn:aws:ec2:*:*:snapshot/*"
        },
        {
          "Action": [
            "ec2:DeleteVolume"
          ],
          "Condition": {
            "Null": {
              "aws:ResourceTag/AWSApplicationMigrationServiceManaged": "false"
            }
          },
          "Effect": "Allow",
          "Resource": "arn:aws:ec2:*:*:volume/*"
        },
        {
          "Action": [
            "ec2:GetConsoleOutput",
            "ec2:GetConsoleScreenshot",
            "ec2:ModifyInstanceAttribute",
            "ec2:StartInstances",
            "ec2:StopInstances",
            "ec2:TerminateInstances"
          ],
          "Condition": {
            "Null": {
              "aws:ResourceTag/AWSApplicationMigrationServiceManaged": "false"
            }
          },
          "Effect": "Allow",
          "Resource": "arn:aws:ec2:*:*:instance/*"
        },
        {
          "Action": [
            "ec2:RunInstances"
          ],
          "Condition": {
            "Null": {
              "aws:RequestTag/AWSApplicationMigrationServiceManaged": "false"
            }
          },
          "Effect": "Allow",
          "Resource": "arn:aws:ec2:*:*:instance/*"
        },
        {
          "Action": "kms:ListRetirableGrants",
          "Effect": "Allow",
          "Resource": "*"
        },
        {
          "Action": "mgn:ListTagsForResource",
          "Effect": "Allow",
          "Resource": "*"
        },
        {
          "Action": [
            "ec2:CreateSecurityGroup"
          ],
          "Effect": "Allow",
          "Resource": "arn:aws:ec2:*:*:vpc/*"
        },
        {
          "Action": [
            "ec2:DeregisterImage",
            "ec2:RegisterImage"
          ],
          "Effect": "Allow",
          "Resource": "*"
        },
        {
          "Action": [
            "ec2:DescribeAccountAttributes",
            "ec2:DescribeAvailabilityZones",
            "ec2:DescribeImages",
            "ec2:DescribeInstanceAttribute",
            "ec2:DescribeInstanceStatus",
            "ec2:DescribeInstanceTypes",
            "ec2:DescribeInstances",
            "ec2:DescribeLaunchTemplateVersions",
            "ec2:DescribeLaunchTemplates",
            "ec2:DescribeSecurityGroups",
            "ec2:DescribeSnapshots",
            "ec2:DescribeSubnets",
            "ec2:DescribeVolumes",
            "ec2:GetEbsDefaultKmsKeyId",
            "ec2:GetEbsEncryptionByDefault"
          ],
          "Effect": "Allow",
          "Resource": "*"
        },
        {
          "Action": [
            "ec2:DetachVolume"
          ],
          "Effect": "Allow",
          "Resource": "arn:aws:ec2:*:*:volume/*"
        },
        {
          "Action": [
            "ec2:RunInstances"
          ],
          "Effect": "Allow",
          "Resource": [
            "arn:aws:ec2:*:*:image/*",
            "arn:aws:ec2:*:*:launch-template/*",
            "arn:aws:ec2:*:*:network-interface/*",
            "arn:aws:ec2:*:*:security-group/*",
            "arn:aws:ec2:*:*:subnet/*",
            "arn:aws:ec2:*:*:volume/*"
          ]
        },
        {
          "Action": [
            "mgh:AssociateCreatedArtifact",
            "mgh:CreateProgressUpdateStream",
            "mgh:DisassociateCreatedArtifact",
            "mgh:GetHomeRegion",
            "mgh:ImportMigrationTask",
            "mgh:NotifyMigrationTaskState",
            "mgh:PutResourceAttributes"
          ],
          "Effect": "Allow",
          "Resource": "*"
        }
      ],
      "Version": "2012-10-17"
    },
    "IsDefaultVersion": true,
    "VersionId": "v1"
  },
  "UpdateDate": "2021-04-07T06:43:20Z"
}
