{
  "Arn": "arn:aws:iam::aws:policy/AmazonFSxConsoleFullAccess",
  "CreateDate": "2018-11-28T16:36:05Z",
  "DefaultVersionId": "v6",
  "IsAttachable": true,
  "Path": "/",
  "PolicyId": "ANPAITDDJ23Y5UZ2WCZRQ",
  "PolicyName": "AmazonFSxConsoleFullAccess",
  "PolicyVersion": {
    "CreateDate": "2022-09-22T14:08:01Z",
    "Document": {
      "Statement": [
        {
          "Action": "iam:CreateServiceLinkedRole",
          "Condition": {
            "StringEquals": {
              "iam:AWSServiceName": [
                "fsx.amazonaws.com"
              ]
            }
          },
          "Effect": "Allow",
          "Resource": "*"
        },
        {
          "Action": "iam:CreateServiceLinkedRole",
          "Condition": {
            "StringEquals": {
              "iam:AWSServiceName": [
                "s3.data-source.lustre.fsx.amazonaws.com"
              ]
            }
          },
          "Effect": "Allow",
          "Resource": "*"
        },
        {
          "Action": [
            "ec2:CreateTags"
          ],
          "Condition": {
            "ForAnyValue:StringEquals": {
              "aws:CalledVia": [
                "fsx.amazonaws.com"
              ]
            },
            "StringEquals": {
              "aws:RequestTag/AmazonFSx": "ManagedByAmazonFSx"
            }
          },
          "Effect": "Allow",
          "Resource": [
            "arn:aws:ec2:*:*:route-table/*"
          ]
        },
        {
          "Action": [
            "cloudwatch:DescribeAlarms",
            "cloudwatch:GetMetricData",
            "ds:DescribeDirectories",
            "ec2:DescribeNetworkInterfaceAttribute",
            "ec2:DescribeRouteTables",
            "ec2:DescribeSecurityGroups",
            "ec2:DescribeSubnets",
            "ec2:DescribeVpcs",
            "firehose:ListDeliveryStreams",
            "fsx:*",
            "kms:ListAliases",
            "logs:DescribeLogGroups",
            "s3:ListBucket"
          ],
          "Effect": "Allow",
          "Resource": "*"
        }
      ],
      "Version": "2012-10-17"
    },
    "IsDefaultVersion": true,
    "VersionId": "v6"
  },
  "UpdateDate": "2022-09-22T14:08:01Z"
}
