{
  "Arn": "arn:aws:iam::aws:policy/SageMakerStudioAdminIAMPermissiveExecutionPolicy",
  "CreateDate": "2025-08-18T17:19:07Z",
  "DefaultVersionId": "v4",
  "IsAttachable": true,
  "Path": "/",
  "PolicyId": "ANPAZKAPJZG4DPB4UTXYO",
  "PolicyName": "SageMakerStudioAdminIAMPermissiveExecutionPolicy",
  "PolicyVersion": {
    "CreateDate": "2025-11-15T03:04:09Z",
    "Document": {
      "Statement": [
        {
          "Action": "iam:AttachRolePolicy",
          "Condition": {
            "ArnEquals": {
              "iam:PolicyARN": [
                "arn:aws:iam::aws:policy/SageMakerStudioUserIAMDefaultExecutionPolicy",
                "arn:aws:iam::aws:policy/SageMakerStudioUserIAMPermissiveExecutionPolicy",
                "arn:aws:iam::aws:policy/service-role/AmazonS3TablesLakeFormationServiceRole"
              ]
            }
          },
          "Effect": "Allow",
          "Resource": "arn:aws:iam::*:role/service-role/AmazonSageMaker*",
          "Sid": "AttachPolicy"
        },
        {
          "Action": "iam:PassRole",
          "Condition": {
            "StringEquals": {
              "aws:ResourceAccount": "${aws:PrincipalAccount}",
              "iam:PassedToService": [
                "airflow-serverless.amazonaws.com",
                "athena.amazonaws.com",
                "datazone.amazonaws.com",
                "glue.amazonaws.com",
                "lakeformation.amazonaws.com",
                "sagemaker.amazonaws.com"
              ]
            }
          },
          "Effect": "Allow",
          "Resource": "*",
          "Sid": "PassRoleForProvisioning"
        },
        {
          "Action": "iam:PassRole",
          "Condition": {
            "StringEquals": {
              "iam:PassedToService": [
                "airflow-serverless.amazonaws.com",
                "bedrock.amazonaws.com",
                "datazone.amazonaws.com",
                "emr-serverless.amazonaws.com",
                "redshift-serverless.amazonaws.com",
                "redshift.amazonaws.com",
                "scheduler.amazonaws.com"
              ]
            }
          },
          "Effect": "Allow",
          "Resource": [
            "arn:aws:iam::*:role/${aws:PrincipalTag/AmazonDataZonePassedRolePath}",
            "arn:aws:iam::*:role/service-role/AmazonSageMaker*"
          ],
          "Sid": "PassRole"
        },
        {
          "Action": "sts:SetSourceIdentity",
          "Condition": {
            "StringLike": {
              "sts:SourceIdentity": "${aws:PrincipalTag/datazone:userId}"
            }
          },
          "Effect": "Allow",
          "Resource": "*",
          "Sid": "SourceIdentity"
        },
        {
          "Action": [
            "athena:GetResourceDashboard",
            "athena:GetSession",
            "athena:GetSessionEndpoint",
            "athena:GetSessionStatus",
            "athena:StartSession",
            "athena:TerminateSession"
          ],
          "Condition": {
            "StringNotEquals": {
              "aws:RequestTag/AmazonDataZoneSessionOwner": "${aws:SourceIdentity}",
              "aws:ResourceTag/AmazonDataZoneSessionOwner": "${aws:SourceIdentity}"
            }
          },
          "Effect": "Deny",
          "Resource": [
            "arn:aws:athena:*:*:workgroup/*/session/*"
          ],
          "Sid": "AthenaSessionIsolation"
        },
        {
          "Action": [
            "athena:TagResource",
            "athena:UntagResource"
          ],
          "Condition": {
            "StringNotEquals": {
              "aws:ResourceTag/AmazonDataZoneSessionOwner": "${aws:SourceIdentity}"
            }
          },
          "Effect": "Deny",
          "Resource": "arn:aws:athena:*:*:workgroup/*/session/*",
          "Sid": "DenyTaggingUntaggingForeignAthenaSessions"
        },
        {
          "Action": [
            "ec2:AuthorizeSecurityGroupEgress",
            "ec2:AuthorizeSecurityGroupIngress"
          ],
          "Condition": {
            "Null": {
              "aws:ResourceTag/AmazonDataZoneProject": "false"
            }
          },
          "Effect": "Allow",
          "Resource": [
            "arn:aws:ec2:*:*:security-group/*"
          ],
          "Sid": "SGAuth"
        },
        {
          "Action": [
            "ec2:CreateTags"
          ],
          "Condition": {
            "ForAllValues:StringLike": {
              "aws:TagKeys": [
                "AmazonDataZone*",
                "aws:cloudformation:*"
              ]
            }
          },
          "Effect": "Allow",
          "Resource": "arn:aws:ec2:*:*:security-group/*",
          "Sid": "SGCreateTags"
        },
        {
          "Action": [
            "glue:CancelStatement",
            "glue:CreateSession",
            "glue:DeleteSession",
            "glue:GetDashboardUrl",
            "glue:GetSession",
            "glue:GetStatement",
            "glue:RunStatement",
            "glue:StopSession"
          ],
          "Condition": {
            "StringNotEquals": {
              "aws:RequestTag/AmazonDataZoneSessionOwner": "${aws:SourceIdentity}",
              "aws:ResourceTag/AmazonDataZoneSessionOwner": "${aws:SourceIdentity}"
            }
          },
          "Effect": "Deny",
          "Resource": [
            "arn:aws:glue:*:*:session/*"
          ],
          "Sid": "GlueSessionIsolation"
        },
        {
          "Action": [
            "glue:TagResource",
            "glue:UntagResource"
          ],
          "Condition": {
            "StringNotEquals": {
              "aws:ResourceTag/AmazonDataZoneSessionOwner": "${aws:SourceIdentity}"
            }
          },
          "Effect": "Deny",
          "Resource": "arn:aws:glue:*:*:session/*",
          "Sid": "DenyTaggingUntaggingForeignSessions"
        },
        {
          "Action": [
            "iam:TagRole",
            "sts:TagSession"
          ],
          "Condition": {
            "ForAllValues:StringLike": {
              "aws:TagKeys": [
                "AmazonDataZone*"
              ]
            }
          },
          "Effect": "Allow",
          "Resource": "*",
          "Sid": "TagRoleAndSession"
        },
        {
          "Action": [
            "kms:CreateGrant"
          ],
          "Condition": {
            "ForAllValues:StringEquals": {
              "kms:GrantOperations": [
                "kms:Decrypt",
                "kms:Encrypt",
                "kms:GenerateDataKey",
                "kms:GenerateDataKeyWithoutPlaintext"
              ]
            },
            "ForAnyValue:StringEquals": {
              "kms:EncryptionContextKeys": "aws:airflow-serverless:workflow-arn"
            },
            "StringLike": {
              "kms:ViaService": "airflow-serverless.*.amazonaws.com"
            }
          },
          "Effect": "Allow",
          "Resource": "arn:*:kms:*:*:key/*",
          "Sid": "WorkflowsCreateGrant"
        },
        {
          "Action": [
            "kms:CreateGrant"
          ],
          "Condition": {
            "StringLike": {
              "kms:ViaService": "sagemaker.*.amazonaws.com"
            }
          },
          "Effect": "Allow",
          "Resource": "*",
          "Sid": "SageMakerCreateGrant"
        },
        {
          "Action": [
            "kms:CreateGrant",
            "kms:Decrypt",
            "kms:GenerateDataKey"
          ],
          "Condition": {
            "Null": {
              "kms:EncryptionContextKeys": "false"
            },
            "StringLike": {
              "kms:ViaService": "bedrock.*.amazonaws.com"
            }
          },
          "Effect": "Allow",
          "Resource": "*",
          "Sid": "BedrockKms"
        },
        {
          "Action": [
            "kms:Decrypt",
            "kms:Encrypt",
            "kms:GenerateDataKey"
          ],
          "Condition": {
            "Null": {
              "kms:EncryptionContext:SecretARN": "false"
            },
            "StringLike": {
              "kms:ViaService": "secretsmanager.*.amazonaws.com"
            }
          },
          "Effect": "Allow",
          "Resource": "*",
          "Sid": "SecretsKms"
        },
        {
          "Action": [
            "kms:Decrypt",
            "kms:Encrypt",
            "kms:GenerateDataKey",
            "kms:GenerateDataKeyWithoutPlaintext"
          ],
          "Condition": {
            "ForAnyValue:StringEquals": {
              "kms:EncryptionContextKeys": "aws:airflow-serverless:workflow-arn"
            },
            "StringLike": {
              "kms:ViaService": "airflow-serverless.*.amazonaws.com"
            }
          },
          "Effect": "Allow",
          "Resource": "arn:*:kms:*:*:key/*",
          "Sid": "WorkflowsKms"
        },
        {
          "Action": [
            "kms:Decrypt",
            "kms:Encrypt",
            "kms:GenerateDataKey",
            "kms:GenerateDataKeyWithoutPlaintext"
          ],
          "Condition": {
            "Null": {
              "kms:EncryptionContextKeys": "false"
            },
            "StringLike": {
              "kms:ViaService": "glue.*.amazonaws.com"
            }
          },
          "Effect": "Allow",
          "Resource": "*",
          "Sid": "GlueKms"
        },
        {
          "Action": [
            "kms:Decrypt",
            "kms:Encrypt",
            "kms:GenerateDataKey",
            "kms:GenerateDataKeyWithoutPlaintext",
            "kms:ReEncryptFrom",
            "kms:ReEncryptTo"
          ],
          "Condition": {
            "Null": {
              "kms:EncryptionContextKeys": "false"
            },
            "StringLike": {
              "kms:ViaService": "sagemaker.*.amazonaws.com"
            }
          },
          "Effect": "Allow",
          "Resource": "*",
          "Sid": "SageMakerKms"
        },
        {
          "Action": [
            "kms:Decrypt",
            "kms:GenerateDataKey"
          ],
          "Condition": {
            "ForAnyValue:StringEquals": {
              "kms:EncryptionContextKeys": "aws:datazone:domainId"
            },
            "StringLike": {
              "kms:ViaService": "datazone.*.amazonaws.com"
            }
          },
          "Effect": "Allow",
          "Resource": "*",
          "Sid": "DataZoneKms"
        },
        {
          "Action": [
            "kms:Decrypt",
            "kms:GenerateDataKey"
          ],
          "Condition": {
            "Null": {
              "kms:EncryptionContext:aws:s3:arn": "false"
            },
            "StringLike": {
              "kms:ViaService": "s3.*.amazonaws.com"
            }
          },
          "Effect": "Allow",
          "Resource": "*",
          "Sid": "S3Kms"
        },
        {
          "Action": [
            "kms:Decrypt",
            "kms:GenerateDataKey"
          ],
          "Condition": {
            "Null": {
              "kms:EncryptionContext:aws:scheduler:schedule:arn": "false"
            },
            "StringLike": {
              "kms:ViaService": "scheduler.*.amazonaws.com"
            }
          },
          "Effect": "Allow",
          "Resource": "*",
          "Sid": "SchedulerKms"
        },
        {
          "Action": [
            "secretsmanager:CreateSecret",
            "secretsmanager:DeleteSecret",
            "secretsmanager:DescribeSecret",
            "secretsmanager:GetSecretValue",
            "secretsmanager:PutResourcePolicy",
            "secretsmanager:TagResource",
            "secretsmanager:UpdateSecret"
          ],
          "Condition": {
            "Null": {
              "aws:ResourceTag/AmazonDataZoneProject": "false"
            }
          },
          "Effect": "Allow",
          "Resource": "*",
          "Sid": "ManagePrivateSecret"
        },
        {
          "Action": [
            "secretsmanager:CreateSecret",
            "secretsmanager:DeleteSecret",
            "secretsmanager:DescribeSecret",
            "secretsmanager:GetSecretValue",
            "secretsmanager:TagResource",
            "secretsmanager:UpdateSecret"
          ],
          "Condition": {
            "Null": {
              "aws:ResourceTag/AmazonDataZoneProject": "true"
            },
            "StringEquals": {
              "aws:ResourceTag/for-use-with-all-datazone-projects": "true"
            }
          },
          "Effect": "Allow",
          "Resource": "*",
          "Sid": "ManageSharedSecret"
        },
        {
          "Action": "cloudformation:ValidateTemplate",
          "Effect": "Allow",
          "Resource": "*",
          "Sid": "ValidateCfn"
        },
        {
          "Action": "ec2:Describe*",
          "Effect": "Allow",
          "Resource": "*",
          "Sid": "Ec2DescribeOnly"
        },
        {
          "Action": "iam:CreateServiceLinkedRole",
          "Effect": "Allow",
          "Resource": [
            "arn:aws:iam::*:role/aws-service-role/airflow-serverless.amazonaws.com/AWSServiceRoleForAmazonMWAAServerless",
            "arn:aws:iam::*:role/aws-service-role/airflow.amazonaws.com/AWSServiceRoleForAmazonMWAA",
            "arn:aws:iam::*:role/aws-service-role/elasticmapreduce.amazonaws.com/AWSServiceRoleForEMRCleanup",
            "arn:aws:iam::*:role/aws-service-role/neptune-graph.amazonaws.com/AWSServiceRoleForNeptuneGraph",
            "arn:aws:iam::*:role/aws-service-role/observability.aoss.amazonaws.com/AWSServiceRoleForAmazonOpenSearchServerless",
            "arn:aws:iam::*:role/aws-service-role/ops.athena.amazonaws.com/AWSServiceRoleForAmazonAthena",
            "arn:aws:iam::*:role/aws-service-role/ops.emr-serverless.amazonaws.com/AWSServiceRoleForAmazonEMRServerless",
            "arn:aws:iam::*:role/aws-service-role/redshift.amazonaws.com/AWSServiceRoleForRedshift",
            "arn:aws:iam::*:role/aws-service-role/sagemaker.amazonaws.com/AWSServiceRoleForAmazonSageMakerNotebooks",
            "arn:aws:iam::*:role/aws-service-role/sagemaker.application-autoscaling.amazonaws.com/AWSServiceRoleForApplicationAutoScaling_SageMakerEndpoint"
          ],
          "Sid": "CreateSLR"
        },
        {
          "Action": [
            "airflow-serverless:*",
            "athena:*",
            "bedrock:*",
            "codewhisperer:*",
            "datazone:*",
            "emr-serverless:*",
            "q:*",
            "sagemaker-mlflow:*",
            "sagemaker-unified-studio-mcp:*",
            "sagemaker:*",
            "scheduler:*",
            "sqlworkbench:*"
          ],
          "Effect": "Allow",
          "Resource": "*",
          "Sid": "ComputeAccess"
        },
        {
          "Action": [
            "cloudformation:*"
          ],
          "Effect": "Allow",
          "Resource": [
            "arn:aws:cloudformation:*:*:stack/DataZone*"
          ],
          "Sid": "CfnManage"
        },
        {
          "Action": [
            "cloudwatch:*",
            "glue:*",
            "logs:*",
            "redshift-data:*",
            "redshift-serverless:*",
            "redshift:*",
            "s3:*"
          ],
          "Effect": "Allow",
          "Resource": "*",
          "Sid": "DataAccess"
        },
        {
          "Action": [
            "codeconnections:*",
            "codestar-connections:*"
          ],
          "Effect": "Allow",
          "Resource": "*",
          "Sid": "CodeConnections"
        },
        {
          "Action": [
            "dynamodb:Describe*",
            "dynamodb:List*",
            "dynamodb:PartiQLSelect",
            "dynamodb:Query",
            "dynamodb:Scan",
            "resource-groups:GetGroupQuery",
            "resource-groups:ListGroupResources",
            "secretsmanager:ListSecrets"
          ],
          "Effect": "Allow",
          "Resource": "*",
          "Sid": "FederatedConn"
        },
        {
          "Action": [
            "ec2:CreateNetworkInterface",
            "ec2:CreateNetworkInterfacePermission",
            "ec2:DeleteNetworkInterface",
            "ec2:DeleteNetworkInterfacePermission"
          ],
          "Effect": "Allow",
          "Resource": "*",
          "Sid": "VpcAccess"
        },
        {
          "Action": [
            "ec2:CreateSecurityGroup"
          ],
          "Effect": "Allow",
          "Resource": [
            "arn:aws:ec2:*:*:security-group/*",
            "arn:aws:ec2:*:*:vpc/*"
          ],
          "Sid": "CreateSG"
        },
        {
          "Action": [
            "ec2:CreateTags",
            "ec2:DeleteTags"
          ],
          "Effect": "Allow",
          "Resource": [
            "arn:aws:ec2:*:*:network-interface/*"
          ],
          "Sid": "EC2TagAccessForVpc"
        },
        {
          "Action": [
            "ec2:DeleteSecurityGroup",
            "ec2:RevokeSecurityGroupEgress",
            "ec2:RevokeSecurityGroupIngress"
          ],
          "Effect": "Allow",
          "Resource": [
            "arn:aws:ec2:*:*:security-group/*"
          ],
          "Sid": "SGManage"
        },
        {
          "Action": [
            "ecr:BatchCheckLayerAvailability",
            "ecr:BatchGetImage",
            "ecr:DescribeImages",
            "ecr:GetAuthorizationToken",
            "ecr:GetDownloadUrlForLayer"
          ],
          "Effect": "Allow",
          "Resource": "*",
          "Sid": "Ecr"
        },
        {
          "Action": [
            "iam:CreateRole"
          ],
          "Effect": "Allow",
          "Resource": [
            "arn:aws:iam::*:role/service-role/AmazonSageMaker*"
          ],
          "Sid": "CreateRole"
        },
        {
          "Action": [
            "iam:GetRole",
            "iam:GetUser",
            "iam:ListRoles",
            "iam:ListUsers",
            "sts:AssumeRole"
          ],
          "Effect": "Allow",
          "Resource": "*",
          "Sid": "IamSts"
        },
        {
          "Action": [
            "kms:DescribeKey",
            "kms:ListAliases",
            "kms:ListGrants"
          ],
          "Effect": "Allow",
          "Resource": "*",
          "Sid": "KmsListAndDescribe"
        },
        {
          "Action": [
            "lakeformation:BatchGrantPermissions",
            "lakeformation:BatchRevokePermissions",
            "lakeformation:CreateLakeFormationOptIn",
            "lakeformation:DeleteLakeFormationOptIn",
            "lakeformation:DeregisterResource",
            "lakeformation:DescribeResource",
            "lakeformation:GetDataAccess",
            "lakeformation:GetDataLakeSettings",
            "lakeformation:GrantPermissions",
            "lakeformation:ListLakeFormationOptIns",
            "lakeformation:ListPermissions",
            "lakeformation:ListResources",
            "lakeformation:PutDataLakeSettings",
            "lakeformation:RegisterResource",
            "lakeformation:RevokePermissions"
          ],
          "Effect": "Allow",
          "Resource": "*",
          "Sid": "LFAccess"
        },
        {
          "Action": [
            "secretsmanager:CreateSecret",
            "secretsmanager:DeleteSecret",
            "secretsmanager:DescribeSecret",
            "secretsmanager:RotateSecret",
            "secretsmanager:TagResource",
            "secretsmanager:UpdateSecret"
          ],
          "Effect": "Allow",
          "Resource": "arn:aws:secretsmanager:*:*:secret:redshift!*",
          "Sid": "RedshiftSecret"
        },
        {
          "Action": [
            "ssm:DeleteParameter",
            "ssm:GetParameter*",
            "ssm:PutParameter"
          ],
          "Effect": "Allow",
          "Resource": [
            "arn:aws:ssm:*:*:parameter/amazon/datazone/genAI/*",
            "arn:aws:ssm:*:*:parameter/amazon/datazone/q*",
            "arn:aws:ssm:*::parameter/aws/service/sagemaker-distribution/*"
          ],
          "Sid": "SSM"
        }
      ],
      "Version": "2012-10-17"
    },
    "IsDefaultVersion": true,
    "VersionId": "v4"
  },
  "UpdateDate": "2025-11-15T03:04:09Z"
}
