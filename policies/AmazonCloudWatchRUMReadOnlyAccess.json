{
  "Arn": "arn:aws:iam::aws:policy/AmazonCloudWatchRUMReadOnlyAccess",
  "CreateDate": "2021-11-29T15:43:47Z",
  "DefaultVersionId": "v8",
  "IsAttachable": true,
  "Path": "/",
  "PolicyId": "ANPAZKAPJZG4MNS2SANVJ",
  "PolicyName": "AmazonCloudWatchRUMReadOnlyAccess",
  "PolicyVersion": {
    "CreateDate": "2026-02-12T18:02:30Z",
    "Document": {
      "Statement": [
        {
          "Action": [
            "cloudwatch:DescribeAlarms"
          ],
          "Effect": "Allow",
          "Resource": "arn:aws:cloudwatch:*:*:alarm:*"
        },
        {
          "Action": [
            "cloudwatch:GetMetricData"
          ],
          "Effect": "Allow",
          "Resource": "*"
        },
        {
          "Action": [
            "logs:DescribeLogGroups"
          ],
          "Effect": "Allow",
          "Resource": "arn:aws:logs:*:*:log-group::log-stream:*"
        },
        {
          "Action": [
            "rum:BatchGetRumMetricDefinitions",
            "rum:GetAppMonitor",
            "rum:GetAppMonitorData",
            "rum:GetResourcePolicy",
            "rum:ListAppMonitors",
            "rum:ListRumMetricsDestinations",
            "rum:ListTagsForResource"
          ],
          "Effect": "Allow",
          "Resource": "*"
        },
        {
          "Action": [
            "synthetics:describeCanaries",
            "synthetics:describeCanariesLastRun"
          ],
          "Effect": "Allow",
          "Resource": "arn:aws:synthetics:*:*:canary:*"
        },
        {
          "Action": [
            "xray:GetTraceSummaries"
          ],
          "Effect": "Allow",
          "Resource": "*"
        }
      ],
      "Version": "2012-10-17"
    },
    "IsDefaultVersion": true,
    "VersionId": "v8"
  },
  "UpdateDate": "2026-02-12T18:02:30Z"
}
