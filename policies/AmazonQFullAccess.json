{
  "Arn": "arn:aws:iam::aws:policy/AmazonQFullAccess",
  "CreateDate": "2023-11-28T16:00:24Z",
  "DefaultVersionId": "v4",
  "IsAttachable": true,
  "Path": "/",
  "PolicyId": "ANPAZKAPJZG4JQANMNF6S",
  "PolicyName": "AmazonQFullAccess",
  "PolicyVersion": {
