{
  "Arn": "arn:aws:iam::aws:policy/AmazonQFullAccess",
  "CreateDate": "2023-11-28T16:00:24Z",
  "DefaultVersionId": "v11",
  "IsAttachable": true,
  "Path": "/",
  "PolicyId": "ANPAZKAPJZG4JQANMNF6S",
  "PolicyName": "AmazonQFullAccess",
  "PolicyVersion": {
    "CreateDate": "2026-02-12T18:00:32Z",
    "Document": {
      "Statement": [
        {
          "Action": [
            "iam:PassRole"
          ],
          "Condition": {
            "StringEquals": {
              "iam:PassedToService": [
                "q.amazonaws.com"
              ]
            }
          },
          "Effect": "Allow",
          "Resource": "arn:aws:iam::*:role/*",
          "Sid": "AllowPassRoleToAmazonQ"
        },
        {
          "Action": [
            "cloudformation:GetResource",
            "cloudformation:ListResources"
          ],
          "Effect": "Allow",
          "Resource": "*",
          "Sid": "AllowCloudControlReadAccess"
        },
        {
          "Action": [
            "q:CreateAssignment",
            "q:CreatePlugin",
            "q:DeleteAssignment",
            "q:DeleteConversation",
            "q:DeletePlugin",
            "q:GenerateCodeFromCommands",
            "q:GetConversation",
            "q:GetIdentityMetadata",
            "q:GetPlugin",
            "q:GetTroubleshootingResults",
            "q:ListConversations",
            "q:ListPluginProviders",
            "q:ListPlugins",
            "q:ListTagsForResource",
            "q:PassRequest",
            "q:SendMessage",
            "q:StartConversation",
            "q:StartTroubleshootingAnalysis",
            "q:StartTroubleshootingResolutionExplanation",
            "q:TagResource",
            "q:UntagResource",
            "q:UpdateConversation",
            "q:UpdatePlugin",
            "q:UpdateTroubleshootingCommandResult",
            "q:UsePlugin"
          ],
          "Effect": "Allow",
          "Resource": "*",
          "Sid": "AllowAmazonQFullAccess"
        },
        {
          "Action": [
            "sts:SetContext"
          ],
          "Effect": "Allow",
          "Resource": "arn:aws:sts::*:self",
          "Sid": "AllowSetTrustedIdentity"
        }
      ],
      "Version": "2012-10-17"
    },
    "IsDefaultVersion": true,
    "VersionId": "v11"
  },
  "UpdateDate": "2026-02-12T18:00:32Z"
}
