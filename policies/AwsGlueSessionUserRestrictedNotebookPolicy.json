{
  "Arn": "arn:aws:iam::aws:policy/AwsGlueSessionUserRestrictedNotebookPolicy",
  "CreateDate": "2022-04-18T15:24:56Z",
  "DefaultVersionId": "v2",
  "IsAttachable": true,
  "Path": "/",
  "PolicyId": "ANPAZKAPJZG4BZZRVUUJU",
  "PolicyName": "AwsGlueSessionUserRestrictedNotebookPolicy",
  "PolicyVersion": {
    "CreateDate": "2023-11-22T01:32:43Z",
    "Document": {
      "Statement": [
        {
          "Action": [
            "glue:CancelStatement",
            "glue:DeleteSession",
            "glue:GetSession",
            "glue:GetStatement",
            "glue:ListStatements",
            "glue:RunStatement",
            "glue:StopSession"
          ],
          "Condition": {
            "StringEquals": {
              "aws:ResourceTag/owner": "${aws:PrincipalTag/owner}"
            }
          },
          "Effect": "Allow",
          "Resource": [
            "arn:aws:glue:*:*:session/*"
          ],
          "Sid": "NotebookAllowActions2"
        },
        {
          "Action": [
            "glue:CreateSession"
          ],
          "Condition": {
            "ForAnyValue:StringEquals": {
              "aws:TagKeys": [
                "owner"
              ]
            },
            "StringEquals": {
              "aws:RequestTag/owner": "${aws:PrincipalTag/owner}"
            }
          },
          "Effect": "Allow",
          "Resource": [
            "arn:aws:glue:*:*:session/*"
          ],
          "Sid": "NotebokAllowActions0"
        },
        {
          "Action": [
            "glue:TagResource",
            "glue:UntagResource",
            "tag:TagResources",
            "tag:UntagResources"
          ],
          "Condition": {
            "ForAnyValue:StringEquals": {
              "aws:TagKeys": [
                "owner"
              ]
            }
          },
          "Effect": "Deny",
          "Resource": [
            "arn:aws:glue:*:*:session/*"
          ],
          "Sid": "NotebookDenyActions"
        },
        {
          "Action": [
            "iam:PassRole"
          ],
          "Condition": {
            "StringLike": {
              "iam:PassedToService": [
                "glue.amazonaws.com"
              ]
            }
          },
          "Effect": "Allow",
          "Resource": [
            "arn:aws:iam::*:role/service-role/AwsGlueSessionServiceRoleUserRestrictedForNotebook*"
          ],
          "Sid": "NotebookPassRole"
        },
        {
          "Action": [
            "glue:GetCompletion",
            "glue:StartCompletion"
          ],
          "Effect": "Allow",
          "Resource": [
            "arn:aws:glue:*:*:completion/*"
          ],
          "Sid": "NotebookAllowActions1"
        },
        {
          "Action": [
            "glue:ListSessions"
          ],
          "Effect": "Allow",
          "Resource": [
            "*"
          ],
          "Sid": "NotebookAllowActions3"
        }
      ],
      "Version": "2012-10-17"
    },
    "IsDefaultVersion": true,
    "VersionId": "v2"
  },
  "UpdateDate": "2023-11-22T01:32:43Z"
}
