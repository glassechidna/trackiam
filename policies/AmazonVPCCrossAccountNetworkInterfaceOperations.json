{
  "Arn": "arn:aws:iam::aws:policy/AmazonVPCCrossAccountNetworkInterfaceOperations",
  "CreateDate": "2017-07-18T20:47:16Z",
  "DefaultVersionId": "v5",
  "IsAttachable": true,
  "Path": "/",
  "PolicyId": "ANPAJ53Y4ZY5OHP4CNRJC",
  "PolicyName": "AmazonVPCCrossAccountNetworkInterfaceOperations",
