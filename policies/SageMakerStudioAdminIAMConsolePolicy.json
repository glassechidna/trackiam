{
  "Arn": "arn:aws:iam::aws:policy/SageMakerStudioAdminIAMConsolePolicy",
  "CreateDate": "2025-08-18T22:49:07Z",
  "DefaultVersionId": "v1",
  "IsAttachable": true,
  "Path": "/",
  "PolicyId": "ANPAZKAPJZG4FF4BJSUBG",
  "PolicyName": "SageMakerStudioAdminIAMConsolePolicy",
  "PolicyVersion": {
    "CreateDate": "2025-08-18T22:49:07Z",
    "Document": {
      "Statement": [
        {
          "Action": "iam:PassRole",
          "Condition": {
            "StringEquals": {
              "iam:passedToService": "datazone.amazonaws.com"
            }
          },
          "Effect": "Allow",
          "Resource": [
            "arn:aws:iam::*:role/service-role/AmazonSageMaker*"
          ],
          "Sid": "IAMPassRoleStatement"
        },
        {
          "Action": [
            "datazone:*"
          ],
          "Effect": "Allow",
          "Resource": [
            "*"
          ],
          "Sid": "AmazonDataZoneStatement"
        },
        {
          "Action": [
            "iam:GetRole",
            "iam:GetUser",
            "iam:ListRoles"
          ],
          "Effect": "Allow",
          "Resource": [
            "*"
          ],
          "Sid": "ReadOnlyStatement"
        },
        {
          "Action": [
            "ssm:DeleteParameter",
            "ssm:GetParameter",
            "ssm:GetParametersByPath",
            "ssm:PutParameter"
          ],
          "Effect": "Allow",
          "Resource": [
            "arn:aws:ssm:*:*:parameter/amazon/datazone/q*"
          ],
          "Sid": "SSMParameterStatement"
        }
      ],
      "Version": "2012-10-17"
    },
    "IsDefaultVersion": true,
    "VersionId": "v1"
  },
  "UpdateDate": "2025-08-18T22:49:07Z"
}
