{
  "Arn": "arn:aws:iam::aws:policy/AWSCertificateManagerPrivateCAUser",
  "CreateDate": "2018-10-23T16:53:33Z",
  "DefaultVersionId": "v7",
  "IsAttachable": true,
  "Path": "/",
  "PolicyId": "ANPAJBXCSJJULLMRWSNII",
  "PolicyName": "AWSCertificateManagerPrivateCAUser",
  "PolicyVersion": {
    "CreateDate": "2026-02-12T17:58:48Z",
    "Document": {
      "Statement": [
        {
          "Action": [
            "acm-pca:IssueCertificate"
          ],
          "Condition": {
            "ArnLike": {
              "acm-pca:TemplateArn": [
                "arn:aws:acm-pca:*:*:template/EndEntityCertificate/V*"
              ]
            }
          },
          "Effect": "Allow",
          "Resource": "arn:aws:acm-pca:*:*:certificate-authority/*"
        },
        {
          "Action": [
            "acm-pca:IssueCertificate"
          ],
          "Condition": {
            "ArnNotLike": {
              "acm-pca:TemplateArn": [
                "arn:aws:acm-pca:*:*:template/EndEntityCertificate/V*"
              ]
            }
          },
          "Effect": "Deny",
          "Resource": "arn:aws:acm-pca:*:*:certificate-authority/*"
        },
        {
          "Action": [
            "acm-pca:GetCertificate",
            "acm-pca:ListPermissions",
            "acm-pca:RevokeCertificate"
          ],
          "Effect": "Allow",
          "Resource": "arn:aws:acm-pca:*:*:certificate-authority/*"
        },
        {
          "Action": [
            "acm-pca:ListCertificateAuthorities"
          ],
          "Effect": "Allow",
          "Resource": "*"
        }
      ],
      "Version": "2012-10-17"
    },
    "IsDefaultVersion": true,
    "VersionId": "v7"
  },
  "UpdateDate": "2026-02-12T17:58:48Z"
}
