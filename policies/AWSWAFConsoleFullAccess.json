{
  "Arn": "arn:aws:iam::aws:policy/AWSWAFConsoleFullAccess",
  "CreateDate": "2020-04-06T18:38:38Z",
  "DefaultVersionId": "v7",
  "IsAttachable": true,
  "Path": "/",
  "PolicyId": "ANPAZKAPJZG4AZOTQ7KAT",
  "PolicyName": "AWSWAFConsoleFullAccess",
  "PolicyVersion": {
    "CreateDate": "2023-05-31T19:11:14Z",
    "Document": {
      "Statement": [
        {
          "Action": [
            "logs:PutResourcePolicy"
          ],
          "Condition": {
            "ForAnyValue:StringEquals": {
              "aws:CalledVia": [
                "wafv2.amazonaws.com"
              ]
            }
          },
          "Effect": "Allow",
          "Resource": "*",
          "Sid": "GrantLogDeliveryPermissionForCloudWatchLogGroup"
        },
        {
          "Action": [
            "apigateway:GET",
            "apigateway:SetWebACL",
            "apprunner:AssociateWebAcl",
            "apprunner:DescribeWebAclForService",
            "apprunner:DisassociateWebAcl",
            "apprunner:ListServices",
            "appsync:ListGraphqlApis",
            "appsync:SetWebACL",
            "cloudfront:ListDistributions",
            "cloudfront:ListDistributionsByWebACLId",
            "cloudfront:UpdateDistribution",
            "cloudwatch:GetMetricData",
            "cloudwatch:GetMetricStatistics",
            "cloudwatch:ListMetrics",
            "cognito-idp:AssociateWebACL",
            "cognito-idp:DisassociateWebACL",
            "cognito-idp:GetWebACLForResource",
            "cognito-idp:ListResourcesForWebACL",
            "cognito-idp:ListUserPools",
            "ec2:AssociateVerifiedAccessInstanceWebAcl",
            "ec2:DescribeRegions",
            "ec2:DescribeVerifiedAccessInstanceWebAclAssociations",
            "ec2:DisassociateVerifiedAccessInstanceWebAcl",
            "ec2:GetVerifiedAccessInstanceWebAcl",
            "elasticloadbalancing:DescribeLoadBalancers",
            "elasticloadbalancing:SetWebACL",
            "logs:DescribeLogGroups",
            "logs:DescribeResourcePolicies",
            "s3:ListAllMyBuckets",
            "waf-regional:*",
            "waf:*",
            "wafv2:*"
          ],
          "Effect": "Allow",
          "Resource": "*",
          "Sid": "AllowUseOfAWSWAF"
        },
        {
          "Action": [
            "logs:CreateLogDelivery",
            "logs:DeleteLogDelivery"
          ],
          "Effect": "Allow",
          "Resource": "*",
          "Sid": "AllowLogDeliverySubscription"
        },
        {
          "Action": [
            "s3:GetBucketPolicy",
            "s3:PutBucketPolicy"
          ],
          "Effect": "Allow",
          "Resource": [
            "arn:aws:s3:::aws-waf-logs-*"
          ],
          "Sid": "GrantLogDeliveryPermissionForS3Bucket"
        }
      ],
      "Version": "2012-10-17"
    },
    "IsDefaultVersion": true,
    "VersionId": "v7"
  },
  "UpdateDate": "2023-05-31T19:11:14Z"
}
