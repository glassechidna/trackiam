{
  "Arn": "arn:aws:iam::aws:policy/SageMakerStudioUserIAMConsolePolicy",
  "CreateDate": "2025-08-18T22:49:07Z",
  "DefaultVersionId": "v2",
  "IsAttachable": true,
  "Path": "/",
  "PolicyId": "ANPAZKAPJZG4FQHIZO6RE",
  "PolicyName": "SageMakerStudioUserIAMConsolePolicy",
  "PolicyVersion": {
    "CreateDate": "2025-11-10T22:19:10Z",
    "Document": {
      "Statement": [
        {
          "Action": "iam:PassRole",
          "Condition": {
            "StringEquals": {
              "iam:passedToService": "datazone.amazonaws.com"
            }
          },
          "Effect": "Allow",
          "Resource": [
            "arn:aws:iam::*:role/service-role/AmazonSageMaker*"
          ],
          "Sid": "IAMPassRoleStatement"
        },
        {
          "Action": [
            "kms:Decrypt",
            "kms:GenerateDataKey"
          ],
          "Condition": {
            "ForAnyValue:StringEquals": {
              "kms:EncryptionContextKeys": "aws:datazone:domainId"
            },
            "StringLike": {
              "kms:ViaService": "datazone.*.amazonaws.com"
            }
          },
          "Effect": "Allow",
          "Resource": "*",
          "Sid": "DataZoneKMSPermissions"
        },
        {
          "Action": [
            "datazone:CreateProject",
            "datazone:DeleteProject",
            "datazone:GetDomain",
            "datazone:GetEnvironment",
            "datazone:GetEnvironmentCredentials",
            "datazone:GetGroupProfile",
            "datazone:GetIamPortalLoginUrl",
            "datazone:GetProject",
            "datazone:GetUserProfile",
            "datazone:ListDomains",
            "datazone:ListEnvironmentBlueprints",
            "datazone:ListEnvironments",
            "datazone:ListProjectMemberships",
            "datazone:ListProjectProfiles",
            "datazone:ListProjects",
            "datazone:SearchGroupProfiles",
            "datazone:SearchUserProfiles"
          ],
          "Effect": "Allow",
          "Resource": [
            "*"
          ],
          "Sid": "AmazonDataZoneStatement"
        },
        {
          "Action": [
            "iam:GetRole",
            "iam:GetUser",
            "iam:ListRoles"
          ],
          "Effect": "Allow",
          "Resource": [
            "*"
          ],
          "Sid": "ReadOnlyStatement"
        }
      ],
      "Version": "2012-10-17"
    },
    "IsDefaultVersion": true,
    "VersionId": "v2"
  },
  "UpdateDate": "2025-11-10T22:19:10Z"
}
