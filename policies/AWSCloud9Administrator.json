{
  "Arn": "arn:aws:iam::aws:policy/AWSCloud9Administrator",
  "CreateDate": "2017-11-30T16:17:28Z",
  "DefaultVersionId": "v5",
  "IsAttachable": true,
  "Path": "/",
  "PolicyId": "ANPAIQ4KWP455WDTCBGWK",
  "PolicyName": "AWSCloud9Administrator",
  "PolicyVersion": {
    "CreateDate": "2025-09-25T11:49:08Z",
    "Document": {
      "Statement": [
        {
          "Action": [
            "iam:CreateServiceLinkedRole"
          ],
          "Condition": {
            "StringLike": {
              "iam:AWSServiceName": "cloud9.amazonaws.com"
            }
          },
          "Effect": "Allow",
          "Resource": "*"
        },
        {
          "Action": [
            "ssm:GetConnectionStatus",
            "ssm:StartSession"
          ],
          "Condition": {
            "StringEquals": {
              "aws:CalledViaFirst": "cloud9.amazonaws.com"
            },
            "StringLike": {
              "ssm:resourceTag/aws:cloud9:environment": "*"
            }
          },
          "Effect": "Allow",
          "Resource": "arn:aws:ec2:*:*:instance/*"
        },
        {
          "Action": [
            "cloud9:*",
            "ec2:DescribeInstanceTypeOfferings",
            "ec2:DescribeRouteTables",
            "ec2:DescribeSubnets",
            "ec2:DescribeVpcs",
            "iam:GetUser",
            "iam:ListUsers"
          ],
          "Effect": "Allow",
          "Resource": "*"
        },
        {
          "Action": [
            "ssm:StartSession"
          ],
          "Effect": "Allow",
          "Resource": [
            "arn:aws:ssm:*:*:document/*"
          ]
        },
        {
          "Action": [
            "ssmmessages:OpenDataChannel"
          ],
          "Effect": "Allow",
          "Resource": [
            "arn:aws:ssm:*:*:session/*"
          ]
        }
      ],
      "Version": "2012-10-17"
    },
    "IsDefaultVersion": true,
    "VersionId": "v5"
  },
  "UpdateDate": "2025-09-25T11:49:08Z"
}
