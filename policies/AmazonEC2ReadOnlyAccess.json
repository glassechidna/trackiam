{
  "Arn": "arn:aws:iam::aws:policy/AmazonEC2ReadOnlyAccess",
  "CreateDate": "2015-02-06T18:40:17Z",
  "DefaultVersionId": "v2",
  "IsAttachable": true,
  "Path": "/",
  "PolicyId": "ANPAIGDT4SV4GSETWTBZK",
  "PolicyName": "AmazonEC2ReadOnlyAccess",
  "PolicyVersion": {
    "CreateDate": "2024-02-14T15:17:04Z",
    "Document": {
      "Statement": [
        {
          "Action": "autoscaling:Describe*",
          "Effect": "Allow",
          "Resource": "*",
          "Sid": "Autoscaling"
        },
        {
          "Action": "elasticloadbalancing:Describe*",
          "Effect": "Allow",
          "Resource": "*",
          "Sid": "ELB"
        },
        {
          "Action": [
            "cloudwatch:Describe*",
            "cloudwatch:GetMetricStatistics",
            "cloudwatch:ListMetrics"
          ],
          "Effect": "Allow",
          "Resource": "*",
          "Sid": "Cloudwatch"
        },
        {
          "Action": [
            "ec2:Describe*",
            "ec2:Get*"
          ],
          "Effect": "Allow",
          "Resource": "*",
          "Sid": "EC2"
        }
      ],
      "Version": "2012-10-17"
    },
    "IsDefaultVersion": true,
    "VersionId": "v2"
  },
  "UpdateDate": "2024-02-14T15:17:04Z"
}
