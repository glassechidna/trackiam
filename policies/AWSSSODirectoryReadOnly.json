{
  "Arn": "arn:aws:iam::aws:policy/AWSSSODirectoryReadOnly",
  "CreateDate": "2018-10-31T23:49:32Z",
  "DefaultVersionId": "v7",
  "IsAttachable": true,
  "Path": "/",
  "PolicyId": "ANPAJDPMQELJXZD2NC6JG",
  "PolicyName": "AWSSSODirectoryReadOnly",
  "PolicyVersion": {
    "CreateDate": "2026-02-12T17:58:41Z",
    "Document": {
      "Statement": [
        {
          "Action": [
            "kms:Decrypt"
          ],
          "Condition": {
            "StringLike": {
              "kms:EncryptionContext:aws:identitystore:identitystore-arn": "*",
              "kms:ViaService": "identitystore.*.amazonaws.com"
            }
          },
          "Effect": "Allow",
          "Resource": "*",
          "Sid": "AllowKMSKeyUseViaAWSIdentityStoreService"
        },
        {
          "Action": [
            "identitystore-auth:BatchGetSession",
            "identitystore-auth:ListSessions",
            "identitystore:Describe*",
            "identitystore:List*",
            "sso-directory:Describe*",
            "sso-directory:Get*",
            "sso-directory:List*",
            "sso-directory:Search*"
          ],
          "Effect": "Allow",
          "Resource": "*",
          "Sid": "AWSSSODirectoryReadOnly"
        }
      ],
      "Version": "2012-10-17"
    },
    "IsDefaultVersion": true,
    "VersionId": "v7"
  },
  "UpdateDate": "2026-02-12T17:58:41Z"
}
