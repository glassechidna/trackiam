{
  "Arn": "arn:aws:iam::aws:policy/AmazonAuroraDSQLFullAccess",
  "CreateDate": "2024-12-03T15:36:07Z",
  "DefaultVersionId": "v2",
  "IsAttachable": true,
  "Path": "/",
  "PolicyId": "ANPAZKAPJZG4OL6SKRIYE",
  "PolicyName": "AmazonAuroraDSQLFullAccess",
  "PolicyVersion": {
    "CreateDate": "2025-05-14T19:37:06Z",
    "Document": {
      "Statement": [
        {
          "Action": "iam:CreateServiceLinkedRole",
          "Condition": {
            "StringEquals": {
              "iam:AWSServiceName": "dsql.amazonaws.com"
            }
          },
          "Effect": "Allow",
          "Resource": "*",
          "Sid": "CreateDsqlServiceLinkedRole"
        },
        {
          "Action": [
            "cloudwatch:GetMetricData"
          ],
          "Effect": "Allow",
          "Resource": "*",
          "Sid": "RelatedServicesPermissions"
        },
        {
          "Action": [
            "dsql:AddPeerCluster",
            "dsql:CreateCluster",
            "dsql:CreateMultiRegionClusters",
            "dsql:DbConnect",
            "dsql:DbConnectAdmin",
            "dsql:DeleteCluster",
            "dsql:DeleteMultiRegionClusters",
            "dsql:GetCluster",
            "dsql:GetVpcEndpointServiceName",
            "dsql:ListClusters",
            "dsql:ListTagsForResource",
            "dsql:PutMultiRegionProperties",
            "dsql:PutWitnessRegion",
            "dsql:RemovePeerCluster",
            "dsql:TagResource",
            "dsql:UntagResource",
            "dsql:UpdateCluster"
          ],
          "Effect": "Allow",
          "Resource": "*",
          "Sid": "DsqlAllPermissions"
        }
      ],
      "Version": "2012-10-17"
    },
    "IsDefaultVersion": true,
    "VersionId": "v2"
  },
  "UpdateDate": "2025-05-14T19:37:06Z"
}
