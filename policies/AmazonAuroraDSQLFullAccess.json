{
  "Arn": "arn:aws:iam::aws:policy/AmazonAuroraDSQLFullAccess",
  "CreateDate": "2024-12-03T15:36:07Z",
  "DefaultVersionId": "v15",
  "IsAttachable": true,
  "Path": "/",
  "PolicyId": "ANPAZKAPJZG4OL6SKRIYE",
  "PolicyName": "AmazonAuroraDSQLFullAccess",
  "PolicyVersion": {
    "CreateDate": "2026-02-12T17:59:57Z",
    "Document": {
      "Statement": [
        {
          "Action": "iam:CreateServiceLinkedRole",
          "Condition": {
            "StringEquals": {
              "iam:AWSServiceName": "dsql.amazonaws.com"
            }
          },
          "Effect": "Allow",
          "Resource": "*",
          "Sid": "CreateDsqlServiceLinkedRole"
        },
        {
          "Action": [
            "kms:Decrypt",
            "kms:Encrypt",
            "kms:GenerateDataKey",
            "kms:ReEncryptFrom",
            "kms:ReEncryptTo"
          ],
          "Condition": {
            "ForAnyValue:StringEquals": {
              "kms:EncryptionContextKeys": "aws:dsql:ClusterId"
            },
            "StringLike": {
              "kms:ViaService": [
                "dsql.*.amazonaws.com"
              ]
            }
          },
          "Effect": "Allow",
          "Resource": "*",
          "Sid": "KMSCryptographicPermissions"
        },
        {
          "Action": [
            "kms:DescribeKey"
          ],
          "Condition": {
            "StringLike": {
              "kms:ViaService": [
                "dsql.*.amazonaws.com"
              ]
            }
          },
          "Effect": "Allow",
          "Resource": "*",
          "Sid": "KMSDescribePermission"
        },
        {
          "Action": [
            "cloudwatch:GetMetricData"
          ],
          "Effect": "Allow",
          "Resource": "*",
          "Sid": "RelatedServicesPermissions"
        },
        {
          "Action": [
            "dsql:AddPeerCluster",
            "dsql:CreateCluster",
            "dsql:DbConnect",
            "dsql:DbConnectAdmin",
            "dsql:DeleteCluster",
            "dsql:DeleteClusterPolicy",
            "dsql:GetBackupJob",
            "dsql:GetCluster",
            "dsql:GetClusterPolicy",
            "dsql:GetRestoreJob",
            "dsql:GetVpcEndpointServiceName",
            "dsql:InjectError",
            "dsql:ListClusters",
            "dsql:ListTagsForResource",
            "dsql:PutClusterPolicy",
            "dsql:PutMultiRegionProperties",
            "dsql:PutWitnessRegion",
            "dsql:RemovePeerCluster",
            "dsql:StartBackupJob",
            "dsql:StartRestoreJob",
            "dsql:StopBackupJob",
            "dsql:StopRestoreJob",
            "dsql:TagResource",
            "dsql:UntagResource",
            "dsql:UpdateCluster"
          ],
          "Effect": "Allow",
          "Resource": "*",
          "Sid": "DsqlAllPermissions"
        }
      ],
      "Version": "2012-10-17"
    },
    "IsDefaultVersion": true,
    "VersionId": "v15"
  },
  "UpdateDate": "2026-02-12T17:59:57Z"
}
