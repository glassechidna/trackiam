{
  "Arn": "arn:aws:iam::aws:policy/AWSApplicationMigrationServiceEc2InstancePolicy",
  "CreateDate": "2023-08-22T13:19:02Z",
  "DefaultVersionId": "v1",
  "IsAttachable": true,
  "Path": "/",
