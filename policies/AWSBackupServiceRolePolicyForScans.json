{
  "Arn": "arn:aws:iam::aws:policy/AWSBackupServiceRolePolicyForScans",
  "CreateDate": "2025-11-20T03:34:10Z",
  "DefaultVersionId": "v3",
  "IsAttachable": true,
  "Path": "/",
  "PolicyId": "ANPAZKAPJZG4FA22FHCTC",
  "PolicyName": "AWSBackupServiceRolePolicyForScans",
  "PolicyVersion": {
    "CreateDate": "2026-02-12T18:00:45Z",
    "Document": {
      "Statement": [
        {
          "Action": "iam:PassRole",
          "Condition": {
            "StringEquals": {
              "iam:PassedToService": "malware-protection.guardduty.amazonaws.com"
            }
          },
          "Effect": "Allow",
          "Resource": "*",
          "Sid": "IAMPassPermissions"
        },
        {
          "Action": [
            "ec2:DescribeImages",
            "ec2:DescribeSnapshots"
          ],
          "Effect": "Allow",
          "Resource": "*",
          "Sid": "EC2ReadAPIPermissions"
        },
        {
          "Action": [
            "guardduty:GetMalwareScan",
            "guardduty:StartMalwareScan"
          ],
          "Effect": "Allow",
          "Resource": "*",
          "Sid": "GuardDutyMalwareScanPermissions"
        }
      ],
      "Version": "2012-10-17"
    },
    "IsDefaultVersion": true,
    "VersionId": "v3"
  },
  "UpdateDate": "2026-02-12T18:00:45Z"
}
