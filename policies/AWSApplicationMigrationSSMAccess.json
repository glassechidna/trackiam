{
  "Arn": "arn:aws:iam::aws:policy/AWSApplicationMigrationSSMAccess",
  "CreateDate": "2022-11-27T09:29:05Z",
  "DefaultVersionId": "v5",
  "IsAttachable": true,
  "Path": "/",
  "PolicyId": "ANPAZKAPJZG4EEPTUBLGD",
  "PolicyName": "AWSApplicationMigrationSSMAccess",
  "PolicyVersion": {
    "CreateDate": "2026-02-12T18:00:51Z",
    "Document": {
      "Statement": [
        {
          "Action": [
            "ssm:DescribeDocument",
            "ssm:SendCommand",
            "ssm:StartAutomationExecution"
          ],
          "Condition": {
            "ForAnyValue:StringEquals": {
              "aws:CalledVia": [
                "mgn.amazonaws.com"
              ]
            }
          },
          "Effect": "Allow",
          "Resource": [
            "arn:aws:ssm:*:*:automation-definition/*:*",
            "arn:aws:ssm:*:*:automation-execution/*",
            "arn:aws:ssm:*:*:document/*"
          ]
        },
        {
          "Action": [
            "ssm:DescribeInstanceInformation",
            "ssm:GetCommandInvocation"
          ],
          "Condition": {
            "ForAnyValue:StringEquals": {
              "aws:CalledVia": [
                "mgn.amazonaws.com"
              ]
            }
          },
          "Effect": "Allow",
          "Resource": [
            "*"
          ]
        },
        {
          "Action": [
            "ssm:SendCommand"
          ],
          "Condition": {
            "ForAnyValue:StringEquals": {
              "aws:CalledVia": [
                "mgn.amazonaws.com"
              ]
            },
            "Null": {
              "aws:ResourceTag/AWSApplicationMigrationServiceManaged": "false"
            }
          },
          "Effect": "Allow",
          "Resource": [
            "arn:aws:ec2:*:*:instance/*"
          ]
        },
        {
          "Action": [
            "ssm:GetDocument",
            "ssm:ListDocumentVersions"
          ],
          "Effect": "Allow",
          "Resource": "arn:aws:ssm:*:*:document/*"
        },
        {
          "Action": [
            "ssm:ListDocuments"
          ],
          "Effect": "Allow",
          "Resource": "*"
        }
      ],
      "Version": "2012-10-17"
    },
    "IsDefaultVersion": true,
    "VersionId": "v5"
  },
  "UpdateDate": "2026-02-12T18:00:51Z"
}
