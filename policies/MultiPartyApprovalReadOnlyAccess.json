{
  "Arn": "arn:aws:iam::aws:policy/MultiPartyApprovalReadOnlyAccess",
  "CreateDate": "2025-06-18T20:07:07Z",
  "DefaultVersionId": "v2",
  "IsAttachable": true,
  "Path": "/",
  "PolicyId": "ANPAZKAPJZG4IY6QJCO57",
  "PolicyName": "MultiPartyApprovalReadOnlyAccess",
  "PolicyVersion": {
    "CreateDate": "2025-09-08T17:04:08Z",
    "Document": {
      "Statement": [
        {
          "Action": [
            "kms:Decrypt"
          ],
          "Condition": {
            "ArnLike": {
              "kms:EncryptionContext:aws:identitystore:identitystore-arn": "arn:*:identitystore::*:identitystore/*"
            },
            "StringLike": {
              "kms:ViaService": "identitystore.*.amazonaws.com"
            }
          },
          "Effect": "Allow",
          "Resource": "*",
          "Sid": "AllowKmsAccessViaIdentityStore"
        },
        {
          "Action": [
            "kms:Decrypt"
          ],
          "Condition": {
            "ArnLike": {
              "kms:EncryptionContext:aws:sso:instance-arn": "arn:*:sso:::instance/*"
            },
            "StringLike": {
              "kms:ViaService": "sso.*.amazonaws.com"
            }
          },
          "Effect": "Allow",
          "Resource": "*",
          "Sid": "AllowKmsAccessViaIdentityCenter"
        },
        {
          "Action": [
            "mpa:Get*",
            "mpa:List*"
          ],
          "Effect": "Allow",
          "Resource": "*",
          "Sid": "MpaReadAccess"
        },
        {
          "Action": [
            "organizations:DescribeOrganization",
            "organizations:ListDelegatedAdministrators"
          ],
          "Effect": "Allow",
          "Resource": "*",
          "Sid": "OrganizationsAccess"
        },
        {
          "Action": [
            "sso-directory:DescribeUsers",
            "sso-directory:SearchUsers",
            "sso:DescribeInstance",
            "sso:DescribeRegisteredRegions",
            "sso:GetSharedSsoConfiguration",
            "sso:ListInstances"
          ],
          "Effect": "Allow",
          "Resource": "*",
          "Sid": "SSOManagementAccess"
        }
      ],
      "Version": "2012-10-17"
    },
    "IsDefaultVersion": true,
    "VersionId": "v2"
  },
  "UpdateDate": "2025-09-08T17:04:08Z"
}
