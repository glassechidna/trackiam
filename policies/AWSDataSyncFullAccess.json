{
  "Arn": "arn:aws:iam::aws:policy/AWSDataSyncFullAccess",
  "CreateDate": "2019-01-18T19:40:36Z",
  "DefaultVersionId": "v8",
  "IsAttachable": true,
  "Path": "/",
  "PolicyId": "ANPAJGOHCDUQULZJKDGT4",
  "PolicyName": "AWSDataSyncFullAccess",
  "PolicyVersion": {
    "CreateDate": "2025-04-23T20:07:06Z",
    "Document": {
      "Statement": [
        {
          "Action": "iam:CreateServiceLinkedRole",
          "Condition": {
            "StringEquals": {
              "iam:AWSServiceName": "datasync.amazonaws.com"
            }
          },
          "Effect": "Allow",
          "Resource": "arn:aws:iam::*:role/aws-service-role/datasync.amazonaws.com/AWSServiceRoleForDataSync",
          "Sid": "DataSyncCreateSLRPermissions"
        },
        {
          "Action": [
            "iam:PassRole"
          ],
          "Condition": {
            "StringEquals": {
              "iam:PassedToService": [
                "datasync.amazonaws.com"
              ]
            }
          },
          "Effect": "Allow",
          "Resource": "*",
          "Sid": "DataSyncPassRolePermissions"
        },
        {
          "Action": [
            "datasync:*",
            "ec2:CreateNetworkInterface",
            "ec2:CreateNetworkInterfacePermission",
            "ec2:DeleteNetworkInterface",
            "ec2:DescribeNetworkInterfaces",
            "ec2:DescribeRegions",
            "ec2:DescribeSecurityGroups",
            "ec2:DescribeSubnets",
            "ec2:DescribeVpcEndpoints",
            "ec2:ModifyNetworkInterfaceAttribute",
            "elasticfilesystem:DescribeAccessPoints",
            "elasticfilesystem:DescribeFileSystems",
            "elasticfilesystem:DescribeMountTargets",
            "fsx:DescribeFileSystems",
            "fsx:DescribeStorageVirtualMachines",
            "iam:GetRole",
            "iam:ListRoles",
            "kms:DescribeKey",
            "kms:ListAliases",
            "logs:CreateLogGroup",
            "logs:DescribeLogGroups",
            "logs:DescribeResourcePolicies",
            "outposts:ListOutposts",
            "s3-outposts:ListAccessPoints",
            "s3-outposts:ListRegionalBuckets",
            "s3:GetBucketLocation",
            "s3:ListAllMyBuckets",
            "s3:ListBucket",
            "s3:ListBucketVersions",
            "secretsmanager:ListSecrets"
          ],
          "Effect": "Allow",
          "Resource": "*",
          "Sid": "DataSyncFullAccessPermissions"
        }
      ],
      "Version": "2012-10-17"
    },
    "IsDefaultVersion": true,
    "VersionId": "v8"
  },
  "UpdateDate": "2025-04-23T20:07:06Z"
}
