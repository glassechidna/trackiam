{
  "Arn": "arn:aws:iam::aws:policy/AWSPartnerCentralFullAccess",
  "CreateDate": "2024-11-18T23:33:28Z",
  "DefaultVersionId": "v3",
  "IsAttachable": true,
  "Path": "/",
  "PolicyId": "ANPAZKAPJZG4DRJMXIHNK",
  "PolicyName": "AWSPartnerCentralFullAccess",
  "PolicyVersion": {
    "CreateDate": "2025-11-19T16:34:12Z",
    "Document": {
      "Statement": [
        {
          "Action": [
            "iam:PassRole"
          ],
          "Condition": {
            "StringEquals": {
              "iam:PassedToService": "partnercentral-account-management.amazonaws.com"
            }
          },
          "Effect": "Allow",
          "Resource": "arn:aws:iam::*:role/PartnerCentralRoleFor*",
          "Sid": "PassAWSPartnerCentralRole"
        },
        {
          "Action": [
            "iam:PassRole"
          ],
          "Condition": {
            "StringEquals": {
              "iam:PassedToService": "resource-snapshot-job.partnercentral-selling.amazonaws.com"
            }
          },
          "Effect": "Allow",
          "Resource": [
            "arn:aws:iam::*:role/*"
          ],
          "Sid": "PassAWSPartnerCentralSnapshotJobRole"
        },
        {
          "Action": [
            "partnercentral:*"
          ],
          "Condition": {
            "StringEquals": {
              "partnercentral:Catalog": [
                "AWS",
                "Sandbox"
              ]
            }
          },
          "Effect": "Allow",
          "Resource": "*",
          "Sid": "AWSPartnerCentralAccess"
        },
        {
          "Action": [
            "Partnercentral-account-management:AssociatePartnerUser",
            "Partnercentral-account-management:DisassociatePartnerUser",
            "iam:ListRoles"
          ],
          "Effect": "Allow",
          "Resource": "*",
          "Sid": "PartnerUserRoleAssociation"
        },
        {
          "Action": [
            "sts:AssumeRole"
          ],
          "Effect": "Allow",
          "Resource": [
            "arn:aws:iam::*:role/PartnerCentralChannelBillingTransferManagement",
            "arn:aws:iam::*:role/PartnerCentralChannelBillingTransferReadOnly"
          ],
          "Sid": "ChannelBillingTransferRoleAccess"
        }
      ],
      "Version": "2012-10-17"
    },
    "IsDefaultVersion": true,
    "VersionId": "v3"
  },
  "UpdateDate": "2025-11-19T16:34:12Z"
}
