{
  "Arn": "arn:aws:iam::aws:policy/AmazonSageMakerSpacesRouterPolicy",
  "CreateDate": "2025-11-19T04:34:07Z",
  "DefaultVersionId": "v3",
  "IsAttachable": true,
  "Path": "/",
  "PolicyId": "ANPAZKAPJZG4CWXKKIKNH",
  "PolicyName": "AmazonSageMakerSpacesRouterPolicy",
  "PolicyVersion": {
    "CreateDate": "2026-02-12T17:58:24Z",
    "Document": {
      "Statement": [
        {
          "Action": [
            "kms:Decrypt",
            "kms:GenerateDataKey"
          ],
          "Condition": {
            "StringEquals": {
              "kms:EncryptionContext:sagemaker:component": "amazon-sagemaker-spaces",
              "kms:EncryptionContext:sagemaker:eks-cluster-arn": "${aws:PrincipalTag/eks-cluster-arn}"
            }
          },
          "Effect": "Allow",
          "Resource": "arn:aws:kms:*:*:key/*",
          "Sid": "KMSKeyOperations"
        },
        {
          "Action": [
            "kms:DescribeKey"
          ],
          "Effect": "Allow",
          "Resource": "arn:aws:kms:*:*:key/*",
          "Sid": "KMSDescribeKey"
        }
      ],
      "Version": "2012-10-17"
    },
    "IsDefaultVersion": true,
    "VersionId": "v3"
  },
  "UpdateDate": "2026-02-12T17:58:24Z"
}
