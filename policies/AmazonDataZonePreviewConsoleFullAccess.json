{
  "Arn": "arn:aws:iam::aws:policy/AmazonDataZonePreviewConsoleFullAccess",
  "CreateDate": "2023-03-28T15:16:04Z",
  "DefaultVersionId": "v1",
  "IsAttachable": true,
  "Path": "/",
  "PolicyId": "ANPAZKAPJZG4KCFGJTNUM",
  "PolicyName": "AmazonDataZonePreviewConsoleFullAccess",
  "PolicyVersion": {
    "CreateDate": "2023-03-28T15:16:04Z",
    "Document": {
      "Statement": [
        {
          "Action": "iam:PassRole",
          "Condition": {
            "StringEquals": {
              "iam:passedToService": "datazonecontrol.amazonaws.com"
            }
          },
          "Effect": "Allow",
          "Resource": [
            "arn:aws:iam::*:role/AmazonDataZoneBootstrapRole*",
            "arn:aws:iam::*:role/AmazonDataZoneDomainExecutionRole",
            "arn:aws:iam::*:role/AmazonDataZoneServiceRole*",
            "arn:aws:iam::*:role/service-role/AmazonDataZoneBootstrapRole",
            "arn:aws:iam::*:role/service-role/AmazonDataZoneDomainExecutionRole",
            "arn:aws:iam::*:role/service-role/AmazonDataZoneServiceRole*"
          ]
        },
        {
          "Action": [
            "datazonecontrol:*"
          ],
          "Effect": "Allow",
          "Resource": [
            "*"
          ]
        },
        {
          "Action": [
            "ec2:DescribeSubnets",
            "glue:GetConnections",
            "glue:GetDatabase",
            "iam:ListRoles",
            "kms:DescribeKey",
            "kms:ListAliases",
            "redshift:DescribeClusters",
            "secretsmanager:ListSecrets",
            "sso:DescribeRegisteredRegions"
          ],
          "Effect": "Allow",
          "Resource": [
            "*"
          ]
        },
        {
          "Action": [
            "glue:CreateConnection"
          ],
          "Effect": "Allow",
          "Resource": [
            "arn:aws:glue:*:*:catalog",
            "arn:aws:glue:*:*:connection/AmazonDataZone-*"
          ]
        },
        {
          "Action": [
            "secretsmanager:CreateSecret"
          ],
          "Effect": "Allow",
          "Resource": "arn:aws:secretsmanager:*:*:secret:AmazonDataZone-*"
        }
      ],
      "Version": "2012-10-17"
    },
    "IsDefaultVersion": true,
    "VersionId": "v1"
  },
  "UpdateDate": "2023-03-28T15:16:04Z"
}
