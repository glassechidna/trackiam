{
  "Arn": "arn:aws:iam::aws:policy/aws-service-role/AmazonODBServiceRolePolicy",
  "CreateDate": "2024-11-13T18:21:41Z",
  "DefaultVersionId": "v10",
  "IsAttachable": true,
  "Path": "/aws-service-role/",
  "PolicyId": "ANPAZKAPJZG4BLAIMLXVO",
  "PolicyName": "AmazonODBServiceRolePolicy",
  "PolicyVersion": {
    "CreateDate": "2026-02-12T18:02:51Z",
    "Document": {
      "Statement": [
        {
          "Action": [
            "cloudwatch:PutMetricData"
          ],
          "Condition": {
            "StringEquals": {
              "cloudwatch:namespace": [
                "AWS/ODB"
              ]
            }
          },
          "Effect": "Allow",
          "Resource": "*",
          "Sid": "CloudWatch"
        },
        {
          "Action": [
            "ec2:DescribeAvailabilityZones",
            "ec2:DescribeSubnets",
            "ec2:DescribeTransitGatewayVpcAttachments"
          ],
          "Effect": "Allow",
          "Resource": "*",
          "Sid": "EC2"
        },
        {
          "Action": [
            "events:ActivateEventSource",
            "events:DescribeEventSource"
          ],
          "Effect": "Allow",
          "Resource": "arn:aws:events:*:*:event-source/aws.partner/odb*",
          "Sid": "EB1"
        },
        {
          "Action": [
            "events:CreateEventBus",
            "events:DescribeEventBus"
          ],
          "Effect": "Allow",
          "Resource": "arn:aws:events:*:*:event-bus/aws.partner/odb*",
          "Sid": "EB2"
        },
        {
          "Action": [
            "networkmanager:GetVpcAttachment",
            "networkmanager:ListAttachments"
          ],
          "Effect": "Allow",
          "Resource": "*",
          "Sid": "NM"
        }
      ],
      "Version": "2012-10-17"
    },
    "IsDefaultVersion": true,
    "VersionId": "v10"
  },
  "UpdateDate": "2026-02-12T18:02:51Z"
}
