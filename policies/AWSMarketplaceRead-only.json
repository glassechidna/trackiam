{
  "Arn": "arn:aws:iam::aws:policy/AWSMarketplaceRead-only",
  "CreateDate": "2015-02-06T18:40:31Z",
  "DefaultVersionId": "v11",
  "IsAttachable": true,
  "Path": "/",
