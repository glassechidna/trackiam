          "Effect": "Allow",
          "Resource": "*"
        },
        {
          "Action": [
            "aws-marketplace:ListPrivateListings"
          ],
          "Effect": "Allow",
          "Resource": "*"
        },
        {
          "Action": [
            "iam:ListInstanceProfiles",
            "iam:ListRoles",
            "sns:GetTopicAttributes",
            "sns:ListTopics"
          ],
          "Effect": "Allow",
          "Resource": "*"
        }
      ],
      "Version": "2012-10-17"
    },
