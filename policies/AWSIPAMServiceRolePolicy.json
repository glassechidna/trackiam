{
  "Arn": "arn:aws:iam::aws:policy/aws-service-role/AWSIPAMServiceRolePolicy",
  "CreateDate": "2021-11-30T19:08:11Z",
  "DefaultVersionId": "v6",
  "IsAttachable": true,
  "Path": "/aws-service-role/",
  "PolicyId": "ANPAZKAPJZG4JEQHJ6QN6",
  "PolicyName": "AWSIPAMServiceRolePolicy",
  "PolicyVersion": {
    "CreateDate": "2025-10-23T15:19:07Z",
    "Document": {
      "Statement": [
        {
          "Action": "cloudwatch:PutMetricData",
          "Condition": {
            "StringEquals": {
              "cloudwatch:namespace": "AWS/IPAM"
            }
          },
          "Effect": "Allow",
          "Resource": "*",
          "Sid": "CloudWatchMetricsPublishActions"
        },
        {
          "Action": [
            "ec2:ModifyManagedPrefixList"
          ],
          "Condition": {
            "Null": {
              "ec2:Attribute/ExpectedIpamPrefixListResolverTarget": "false"
            }
          },
          "Effect": "Allow",
          "Resource": [
            "arn:aws:ec2:*:*:prefix-list/pl-*"
          ],
          "Sid": "PrefixListResolverWriteActions"
        },
        {
          "Action": "ec2:AllocateIpamPoolCidr",
          "Effect": "Allow",
          "Resource": "*",
          "Sid": "IPAMAllocationPolicyActions"
        },
        {
          "Action": [
            "cloudfront:ListAnycastIpLists",
            "cloudfront:ListDistributionsByAnycastIpListId",
            "cloudfront:ListTagsForResource",
            "ec2:DescribeAccountAttributes",
            "ec2:DescribeAddresses",
            "ec2:DescribeByoipCidrs",
            "ec2:DescribeIpv6Pools",
            "ec2:DescribeNetworkInterfaces",
            "ec2:DescribePublicIpv4Pools",
            "ec2:DescribeSecurityGroupRules",
            "ec2:DescribeSecurityGroups",
            "ec2:DescribeSubnets",
            "ec2:DescribeVpcs",
            "ec2:DescribeVpnConnections",
            "ec2:GetIpamDiscoveredAccounts",
            "ec2:GetIpamDiscoveredPublicAddresses",
            "ec2:GetIpamDiscoveredResourceCidrs",
            "globalaccelerator:ListAccelerators",
            "globalaccelerator:ListByoipCidrs",
            "organizations:DescribeAccount",
            "organizations:DescribeOrganization",
            "organizations:DescribeOrganizationalUnit",
            "organizations:ListAccounts",
            "organizations:ListChildren",
            "organizations:ListDelegatedAdministrators",
            "organizations:ListParents"
          ],
          "Effect": "Allow",
          "Resource": "*",
          "Sid": "IPAMDiscoveryDescribeActions"
        },
        {
          "Action": [
            "ec2:DescribeManagedPrefixLists",
            "ec2:GetManagedPrefixListEntries"
          ],
          "Effect": "Allow",
          "Resource": [
            "arn:aws:ec2:*:*:prefix-list/pl-*"
          ],
          "Sid": "PrefixListResolverReadActions"
        }
      ],
      "Version": "2012-10-17"
    },
    "IsDefaultVersion": true,
    "VersionId": "v6"
  },
  "UpdateDate": "2025-10-23T15:19:07Z"
}
