{
  "Arn": "arn:aws:iam::aws:policy/SageMakerStudioUserIAMPermissiveExecutionPolicy",
  "CreateDate": "2025-08-18T17:19:08Z",
  "DefaultVersionId": "v15",
  "IsAttachable": true,
  "Path": "/",
  "PolicyId": "ANPAZKAPJZG4MQ2K3CR4F",
  "PolicyName": "SageMakerStudioUserIAMPermissiveExecutionPolicy",
  "PolicyVersion": {
    "CreateDate": "2026-02-12T18:00:47Z",
    "Document": {
      "Statement": [
        {
          "Action": "iam:PassRole",
          "Condition": {
            "StringEquals": {
              "iam:PassedToService": [
                "airflow-serverless.amazonaws.com",
                "bedrock.amazonaws.com",
                "emr-serverless.amazonaws.com",
                "glue.amazonaws.com",
                "lakeformation.amazonaws.com",
                "redshift-serverless.amazonaws.com",
                "redshift.amazonaws.com",
                "sagemaker.amazonaws.com",
                "scheduler.amazonaws.com"
              ]
            }
          },
          "Effect": "Allow",
          "Resource": [
            "arn:aws:iam::*:role/service-role/AmazonSageMaker*"
          ],
          "Sid": "PassRole"
        },
        {
          "Action": "sts:SetSourceIdentity",
          "Condition": {
            "StringLike": {
              "sts:SourceIdentity": "${aws:PrincipalTag/datazone:userId}"
            }
          },
          "Effect": "Allow",
          "Resource": "*",
          "Sid": "SourceIdentity"
        },
        {
          "Action": "sts:TagSession",
          "Condition": {
            "ForAllValues:StringLike": {
              "aws:TagKeys": [
                "AmazonDataZone*"
              ]
            }
          },
          "Effect": "Allow",
          "Resource": "*",
          "Sid": "TagSession"
        },
        {
          "Action": [
            "athena:GetResourceDashboard",
            "athena:GetSession",
            "athena:GetSessionEndpoint",
            "athena:GetSessionStatus",
            "athena:StartSession",
            "athena:TerminateSession"
          ],
          "Condition": {
            "StringNotEquals": {
              "aws:RequestTag/AmazonDataZoneSessionOwner": "${aws:SourceIdentity}",
              "aws:ResourceTag/AmazonDataZoneSessionOwner": "${aws:SourceIdentity}"
            }
          },
          "Effect": "Deny",
          "Resource": [
            "arn:aws:athena:*:*:workgroup/*/session/*"
          ],
          "Sid": "AthenaSessionIsolation"
        },
        {
          "Action": [
            "athena:TagResource",
            "athena:UntagResource"
          ],
          "Condition": {
            "StringNotEquals": {
              "aws:ResourceTag/AmazonDataZoneSessionOwner": "${aws:SourceIdentity}"
            }
          },
          "Effect": "Deny",
          "Resource": "arn:aws:athena:*:*:workgroup/*/session/*",
          "Sid": "DenyTaggingUntaggingForeignAthenaSessions"
        },
        {
          "Action": [
            "glue:CancelStatement",
            "glue:CreateSession",
            "glue:DeleteSession",
            "glue:GetDashboardUrl",
            "glue:GetSession",
            "glue:GetStatement",
            "glue:RunStatement",
            "glue:StopSession"
          ],
          "Condition": {
            "StringNotEquals": {
              "aws:RequestTag/AmazonDataZoneSessionOwner": "${aws:SourceIdentity}",
              "aws:ResourceTag/AmazonDataZoneSessionOwner": "${aws:SourceIdentity}"
            }
          },
          "Effect": "Deny",
          "Resource": [
            "arn:aws:glue:*:*:session/*"
          ],
          "Sid": "GlueSessionIsolation"
        },
        {
          "Action": [
            "glue:TagResource",
            "glue:UntagResource"
          ],
          "Condition": {
            "StringNotEquals": {
              "aws:ResourceTag/AmazonDataZoneSessionOwner": "${aws:SourceIdentity}"
            }
          },
          "Effect": "Deny",
          "Resource": "arn:aws:glue:*:*:session/*",
          "Sid": "DenyTaggingUntaggingForeignSessions"
        },
        {
          "Action": [
            "kms:CreateGrant"
          ],
          "Condition": {
            "ForAllValues:StringEquals": {
              "kms:GrantOperations": [
                "Decrypt",
                "Encrypt",
                "GenerateDataKey",
                "GenerateDataKeyWithoutPlaintext",
                "RetireGrant"
              ]
            },
            "ForAnyValue:StringEquals": {
              "kms:EncryptionContextKeys": "aws:airflow-serverless:workflow-arn"
            },
            "StringLike": {
              "kms:ViaService": "airflow-serverless.*.amazonaws.com"
            }
          },
          "Effect": "Allow",
          "Resource": "arn:*:kms:*:*:key/*",
          "Sid": "WorkflowsCreateGrant"
        },
        {
          "Action": [
            "kms:CreateGrant"
          ],
          "Condition": {
            "ForAllValues:StringEquals": {
              "kms:GrantOperations": [
                "CreateGrant",
                "Decrypt",
                "DescribeKey",
                "Encrypt",
                "GenerateDataKey",
                "GenerateDataKeyWithoutPlaintext",
                "ReEncryptFrom",
                "ReEncryptTo",
                "RetireGrant"
              ]
            },
            "StringLike": {
              "kms:ViaService": "datazone.*.amazonaws.com"
            }
          },
          "Effect": "Allow",
          "Resource": "*",
          "Sid": "DataZoneCreateGrant"
        },
        {
          "Action": [
            "kms:CreateGrant"
          ],
          "Condition": {
            "StringLike": {
              "kms:ViaService": "sagemaker.*.amazonaws.com"
            }
          },
          "Effect": "Allow",
          "Resource": "*",
          "Sid": "SageMakerCreateGrant"
        },
        {
          "Action": [
            "kms:CreateGrant",
            "kms:Decrypt",
            "kms:GenerateDataKey"
          ],
          "Condition": {
            "Null": {
              "kms:EncryptionContextKeys": "false"
            },
            "StringLike": {
              "kms:ViaService": "bedrock.*.amazonaws.com"
            }
          },
          "Effect": "Allow",
          "Resource": "*",
          "Sid": "BedrockKms"
        },
        {
          "Action": [
            "kms:Decrypt",
            "kms:Encrypt",
            "kms:GenerateDataKey"
          ],
          "Condition": {
            "Null": {
              "kms:EncryptionContext:SecretARN": "false"
            },
            "StringLike": {
              "kms:ViaService": "secretsmanager.*.amazonaws.com"
            }
          },
          "Effect": "Allow",
          "Resource": "*",
          "Sid": "SecretsKms"
        },
        {
          "Action": [
            "kms:Decrypt",
            "kms:Encrypt",
            "kms:GenerateDataKey",
            "kms:GenerateDataKeyWithoutPlaintext"
          ],
          "Condition": {
            "ForAnyValue:StringEquals": {
              "kms:EncryptionContextKeys": "aws:airflow-serverless:workflow-arn"
            }
          },
          "Effect": "Allow",
          "Resource": "arn:*:kms:*:*:key/*",
          "Sid": "WorkflowsKms"
        },
        {
          "Action": [
            "kms:Decrypt",
            "kms:Encrypt",
            "kms:GenerateDataKey",
            "kms:GenerateDataKeyWithoutPlaintext"
          ],
          "Condition": {
            "Null": {
              "kms:EncryptionContextKeys": "false"
            },
            "StringLike": {
              "kms:ViaService": "glue.*.amazonaws.com"
            }
          },
          "Effect": "Allow",
          "Resource": "*",
          "Sid": "GlueKms"
        },
        {
          "Action": [
            "kms:Decrypt",
            "kms:Encrypt",
            "kms:GenerateDataKey",
            "kms:GenerateDataKeyWithoutPlaintext",
            "kms:ReEncryptFrom",
            "kms:ReEncryptTo"
          ],
          "Condition": {
            "ForAnyValue:StringEquals": {
              "kms:EncryptionContextKeys": "aws:datazone:domainId"
            },
            "StringLike": {
              "kms:ViaService": "datazone.*.amazonaws.com"
            }
          },
          "Effect": "Allow",
          "Resource": "*",
          "Sid": "DataZoneKms"
        },
        {
          "Action": [
            "kms:Decrypt",
            "kms:Encrypt",
            "kms:GenerateDataKey",
            "kms:GenerateDataKeyWithoutPlaintext",
            "kms:ReEncryptFrom",
            "kms:ReEncryptTo"
          ],
          "Condition": {
            "Null": {
              "kms:EncryptionContextKeys": "false"
            },
            "StringLike": {
              "kms:ViaService": "sagemaker.*.amazonaws.com"
            }
          },
          "Effect": "Allow",
          "Resource": "*",
          "Sid": "SageMakerKms"
        },
        {
          "Action": [
            "kms:Decrypt",
            "kms:GenerateDataKey"
          ],
          "Condition": {
            "Null": {
              "kms:EncryptionContext:aws:scheduler:schedule:arn": "false"
            }
          },
          "Effect": "Allow",
          "Resource": "*",
          "Sid": "SchedulerKms"
        },
        {
          "Action": [
            "kms:Decrypt",
            "kms:GenerateDataKey"
          ],
          "Condition": {
            "Null": {
              "kms:EncryptionContext:aws:s3:arn": "false"
            },
            "StringLike": {
              "kms:ViaService": "s3.*.amazonaws.com"
            }
          },
          "Effect": "Allow",
          "Resource": "*",
          "Sid": "S3Kms"
        },
        {
          "Action": [
            "secretsmanager:CreateSecret",
            "secretsmanager:DeleteSecret",
            "secretsmanager:DescribeSecret",
            "secretsmanager:GetSecretValue",
            "secretsmanager:PutResourcePolicy",
            "secretsmanager:UpdateSecret"
          ],
          "Condition": {
            "StringEquals": {
              "aws:ResourceTag/AmazonDataZoneProject": "${datazone:projectId}"
            }
          },
          "Effect": "Allow",
          "Resource": "*",
          "Sid": "PrivateSecret"
        },
        {
          "Action": [
            "secretsmanager:CreateSecret",
            "secretsmanager:DescribeSecret",
            "secretsmanager:GetSecretValue",
            "secretsmanager:UpdateSecret"
          ],
          "Condition": {
            "StringEquals": {
              "aws:ResourceTag/for-use-with-all-datazone-projects": "true"
            }
          },
          "Effect": "Allow",
          "Resource": "*",
          "Sid": "SharedSecret"
        },
        {
          "Action": "cloudformation:ValidateTemplate",
          "Effect": "Allow",
          "Resource": "*",
          "Sid": "ValidateCfn"
        },
        {
          "Action": "ec2:Describe*",
          "Effect": "Allow",
          "Resource": "*",
          "Sid": "Ec2DescribeOnly"
        },
        {
          "Action": "iam:CreateServiceLinkedRole",
          "Effect": "Allow",
          "Resource": [
            "arn:aws:iam::*:role/aws-service-role/airflow-serverless.amazonaws.com/AWSServiceRoleForAmazonMWAAServerless",
            "arn:aws:iam::*:role/aws-service-role/airflow.amazonaws.com/AWSServiceRoleForAmazonMWAA",
            "arn:aws:iam::*:role/aws-service-role/elasticmapreduce.amazonaws.com/AWSServiceRoleForEMRCleanup",
            "arn:aws:iam::*:role/aws-service-role/neptune-graph.amazonaws.com/AWSServiceRoleForNeptuneGraph",
            "arn:aws:iam::*:role/aws-service-role/observability.aoss.amazonaws.com/AWSServiceRoleForAmazonOpenSearchServerless",
            "arn:aws:iam::*:role/aws-service-role/ops.emr-serverless.amazonaws.com/AWSServiceRoleForAmazonEMRServerless",
            "arn:aws:iam::*:role/aws-service-role/redshift.amazonaws.com/AWSServiceRoleForRedshift",
            "arn:aws:iam::*:role/aws-service-role/sagemaker.amazonaws.com/AWSServiceRoleForAmazonSageMakerNotebooks",
            "arn:aws:iam::*:role/aws-service-role/sagemaker.application-autoscaling.amazonaws.com/AWSServiceRoleForApplicationAutoScaling_SageMakerEndpoint"
          ],
          "Sid": "CreateSLR"
        },
        {
          "Action": [
            "airflow-serverless:*",
            "athena:*",
            "bedrock:*",
            "codewhisperer:*",
            "emr-serverless:*",
            "q:*",
            "sagemaker-mlflow:*",
            "sagemaker-unified-studio-mcp:*",
            "sagemaker:*",
            "scheduler:*",
            "sqlworkbench:*"
          ],
          "Effect": "Allow",
          "Resource": "*",
          "Sid": "ComputeAccess"
        },
        {
          "Action": [
            "cloudformation:*"
          ],
          "Effect": "Allow",
          "Resource": [
            "arn:aws:cloudformation:*:*:stack/DataZone*"
          ],
          "Sid": "CfnManage"
        },
        {
          "Action": [
            "cloudwatch:*",
            "glue:*",
            "logs:*",
            "redshift-data:*",
            "redshift-serverless:*",
            "redshift:*",
            "s3:*"
          ],
          "Effect": "Allow",
          "Resource": "*",
          "Sid": "DataAccess"
        },
        {
          "Action": [
            "codeconnections:GetConnection",
            "codeconnections:GetHost",
            "codeconnections:ListConnections",
            "codeconnections:ListTagsForResource",
            "codeconnections:UseConnection",
            "codestar-connections:GetConnection",
            "codestar-connections:GetHost",
            "codestar-connections:ListConnections",
            "codestar-connections:ListTagsForResource",
            "codestar-connections:UseConnection"
          ],
          "Effect": "Allow",
          "Resource": "*",
          "Sid": "CodeConnectionsUser"
        },
        {
          "Action": [
            "datazone:AcceptPredictions",
            "datazone:AcceptSubscriptionRequest",
            "datazone:BatchGetCell",
            "datazone:BatchGetCellRun",
            "datazone:CancelMetadataGenerationRun",
            "datazone:CancelSubscription",
            "datazone:CreateAsset*",
            "datazone:CreateCell",
            "datazone:CreateCellRun",
            "datazone:CreateConnection",
            "datazone:CreateEnvironment",
            "datazone:CreateListingChangeSet",
            "datazone:CreateNotebook",
            "datazone:CreateProject",
            "datazone:CreateSubscriptionGrant",
            "datazone:CreateSubscriptionRequest",
            "datazone:DeleteAsset*",
            "datazone:DeleteCell",
            "datazone:DeleteCellRun",
            "datazone:DeleteConnection",
            "datazone:DeleteEnvironment",
            "datazone:DeleteListing",
            "datazone:DeleteNotebook",
            "datazone:DeleteProject",
            "datazone:DeleteSubscriptionGrant",
            "datazone:DeleteSubscriptionRequest",
            "datazone:GenerateCode",
            "datazone:Get*",
            "datazone:List*",
            "datazone:PostLineageEvent",
            "datazone:PutCellRunResult",
            "datazone:RejectPredictions",
            "datazone:RejectSubscriptionRequest",
            "datazone:RevokeSubscription",
            "datazone:Search",
            "datazone:SearchGroupProfiles",
            "datazone:SearchListings",
            "datazone:SearchRules",
            "datazone:SearchTypes",
            "datazone:SearchUserProfiles",
            "datazone:SendMessage",
            "datazone:StartConversation",
            "datazone:StartMetadataGenerationRun",
            "datazone:StartNotebookCompute",
            "datazone:StopNotebookCompute",
            "datazone:UpdateAssetFilter",
            "datazone:UpdateCell",
            "datazone:UpdateCellRun",
            "datazone:UpdateConnection",
            "datazone:UpdateEnvironment",
            "datazone:UpdateNotebook",
            "datazone:UpdateProject",
            "datazone:UpdateSubscriptionRequest"
          ],
          "Effect": "Allow",
          "Resource": "*",
          "Sid": "DataZone"
        },
        {
          "Action": [
            "dynamodb:Describe*",
            "dynamodb:List*",
            "dynamodb:PartiQLSelect",
            "dynamodb:Query",
            "dynamodb:Scan",
            "resource-groups:GetGroupQuery",
            "resource-groups:ListGroupResources",
            "secretsmanager:ListSecrets"
          ],
          "Effect": "Allow",
          "Resource": "*",
          "Sid": "FederatedConn"
        },
        {
          "Action": [
            "ec2:CreateNetworkInterface",
            "ec2:CreateNetworkInterfacePermission",
            "ec2:DeleteNetworkInterface",
            "ec2:DeleteNetworkInterfacePermission"
          ],
          "Effect": "Allow",
          "Resource": "*",
          "Sid": "VpcAccess"
        },
        {
          "Action": [
            "ec2:CreateTags",
            "ec2:DeleteTags"
          ],
          "Effect": "Allow",
          "Resource": [
            "arn:aws:ec2:*:*:network-interface/*"
          ],
          "Sid": "EC2TagAccessForVpc"
        },
        {
          "Action": [
            "ecr:BatchCheckLayerAvailability",
            "ecr:BatchGetImage",
            "ecr:DescribeImages",
            "ecr:GetAuthorizationToken",
            "ecr:GetDownloadUrlForLayer"
          ],
          "Effect": "Allow",
          "Resource": "*",
          "Sid": "Ecr"
        },
        {
          "Action": [
            "iam:GetRole",
            "iam:ListRoles",
            "sts:AssumeRole"
          ],
          "Effect": "Allow",
          "Resource": "*",
          "Sid": "IamSts"
        },
        {
          "Action": [
            "kms:DescribeKey",
            "kms:ListAliases",
            "kms:ListGrants"
          ],
          "Effect": "Allow",
          "Resource": "*",
          "Sid": "KmsListAndDescribe"
        },
        {
          "Action": [
            "lakeformation:BatchGrantPermissions",
            "lakeformation:BatchRevokePermissions",
            "lakeformation:DescribeResource",
            "lakeformation:GetDataAccess",
            "lakeformation:GrantPermissions",
            "lakeformation:ListPermissions",
            "lakeformation:ListResources",
            "lakeformation:RevokePermissions"
          ],
          "Effect": "Allow",
          "Resource": "*",
          "Sid": "LFAccess"
        },
        {
          "Action": [
            "ssm:GetParameter*"
          ],
          "Effect": "Allow",
          "Resource": [
            "arn:aws:ssm:*:*:parameter/amazon/datazone/genAI/*",
            "arn:aws:ssm:*:*:parameter/amazon/datazone/q*",
            "arn:aws:ssm:*::parameter/aws/service/sagemaker-distribution/*"
          ],
          "Sid": "SSM"
        }
      ],
      "Version": "2012-10-17"
    },
    "IsDefaultVersion": true,
    "VersionId": "v15"
  },
  "UpdateDate": "2026-02-12T18:00:47Z"
}
