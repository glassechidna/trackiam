{
  "Arn": "arn:aws:iam::aws:policy/aws-service-role/CloudTrailServiceRolePolicy",
  "CreateDate": "2018-10-24T21:21:44Z",
  "DefaultVersionId": "v2",
  "IsAttachable": true,
  "Path": "/aws-service-role/",
  "PolicyId": "ANPAJXQJ45EGU6U7NQBW4",
  "PolicyName": "CloudTrailServiceRolePolicy",
  "PolicyVersion": {
    "CreateDate": "2022-11-03T20:35:01Z",
    "Document": {
      "Statement": [
        {
          "Action": "organizations:ListDelegatedAdministrators",
          "Condition": {
            "StringEquals": {
              "organizations:ServicePrincipal": [
                "cloudtrail.amazonaws.com"
              ]
            }
          },
          "Effect": "Allow",
          "Resource": "*",
          "Sid": "AwsOrgsDelegatedAdminAccess"
        },
        {
          "Action": [
            "cloudtrail:*"
          ],
          "Effect": "Allow",
          "Resource": "*",
          "Sid": "CloudTrailFullAccess"
        },
        {
          "Action": [
            "organizations:DescribeAccount",
            "organizations:DescribeOrganization",
            "organizations:ListAWSServiceAccessForOrganization",
            "organizations:ListAccounts"
          ],
          "Effect": "Allow",
          "Resource": [
            "*"
          ],
          "Sid": "AwsOrgsAccess"
        }
      ],
      "Version": "2012-10-17"
    },
    "IsDefaultVersion": true,
    "VersionId": "v2"
  },
  "UpdateDate": "2022-11-03T20:35:01Z"
}
