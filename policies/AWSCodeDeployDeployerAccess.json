{
  "Arn": "arn:aws:iam::aws:policy/AWSCodeDeployDeployerAccess",
  "CreateDate": "2015-05-19T18:18:43Z",
  "DefaultVersionId": "v6",
  "IsAttachable": true,
  "Path": "/",
  "PolicyId": "ANPAJUWEPOMGLMVXJAPUI",
  "PolicyName": "AWSCodeDeployDeployerAccess",
  "PolicyVersion": {
    "CreateDate": "2026-02-12T18:02:10Z",
    "Document": {
      "Statement": [
        {
          "Action": [
            "codestar-notifications:CreateNotificationRule",
            "codestar-notifications:DescribeNotificationRule",
            "codestar-notifications:Subscribe",
            "codestar-notifications:Unsubscribe",
            "codestar-notifications:UpdateNotificationRule"
          ],
          "Condition": {
            "ArnLike": {
              "codestar-notifications:NotificationsForResource": "arn:aws:codedeploy:*:*:application:*"
            }
          },
          "Effect": "Allow",
          "Resource": "*",
          "Sid": "CodeStarNotificationsReadWriteAccess"
        },
        {
          "Action": [
            "codedeploy:Batch*",
            "codedeploy:CreateDeployment",
            "codedeploy:Get*",
            "codedeploy:List*",
            "codedeploy:RegisterApplicationRevision"
          ],
          "Effect": "Allow",
          "Resource": "*"
        },
        {
          "Action": [
            "chatbot:DescribeSlackChannelConfigurations"
          ],
          "Effect": "Allow",
          "Resource": "*",
          "Sid": "CodeStarNotificationsChatbotAccess"
        },
        {
          "Action": [
            "codestar-notifications:ListEventTypes",
            "codestar-notifications:ListNotificationRules",
            "codestar-notifications:ListTagsforResource",
            "codestar-notifications:ListTargets"
          ],
          "Effect": "Allow",
          "Resource": "*",
          "Sid": "CodeStarNotificationsListAccess"
        },
        {
          "Action": [
            "sns:ListTopics"
          ],
          "Effect": "Allow",
          "Resource": "*",
          "Sid": "SNSTopicListAccess"
        }
      ],
      "Version": "2012-10-17"
    },
    "IsDefaultVersion": true,
    "VersionId": "v6"
  },
  "UpdateDate": "2026-02-12T18:02:10Z"
}
