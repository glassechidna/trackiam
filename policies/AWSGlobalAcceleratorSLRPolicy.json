{
  "Arn": "arn:aws:iam::aws:policy/aws-service-role/AWSGlobalAcceleratorSLRPolicy",
  "CreateDate": "2019-04-05T19:39:13Z",
  "DefaultVersionId": "v7",
  "IsAttachable": true,
  "Path": "/aws-service-role/",
  "PolicyId": "ANPAZKAPJZG4EJ5ZEQR2C",
  "PolicyName": "AWSGlobalAcceleratorSLRPolicy",
  "PolicyVersion": {
    "CreateDate": "2023-05-19T20:41:25Z",
    "Document": {
      "Statement": [
        {
          "Action": [
            "ec2:AssignIpv6Addresses",
            "ec2:DeleteSecurityGroup",
            "ec2:UnassignIpv6Addresses"
          ],
          "Condition": {
            "StringEquals": {
              "ec2:ResourceTag/AWSServiceName": "GlobalAccelerator"
            }
          },
          "Effect": "Allow",
          "Resource": "*",
          "Sid": "EC2Action2"
        },
        {
          "Action": "ec2:CreateTags",
          "Effect": "Allow",
          "Resource": [
            "arn:aws:ec2:*:*:network-interface/*",
            "arn:aws:ec2:*:*:security-group/*"
          ],
          "Sid": "EC2Action4"
        },
        {
          "Action": [
            "ec2:CreateNetworkInterface",
            "ec2:DeleteNetworkInterface",
            "ec2:DescribeAddresses",
            "ec2:DescribeInstances",
            "ec2:DescribeInternetGateways",
            "ec2:DescribeNetworkInterfaces",
            "ec2:DescribeRegions",
            "ec2:DescribeSubnets",
            "ec2:ModifyNetworkInterfaceAttribute"
          ],
          "Effect": "Allow",
          "Resource": "*",
          "Sid": "EC2Action1"
        },
        {
          "Action": [
            "ec2:CreateSecurityGroup",
            "ec2:DescribeSecurityGroups"
          ],
          "Effect": "Allow",
          "Resource": "*",
          "Sid": "EC2Action3"
        },
        {
          "Action": [
            "elasticloadbalancing:DescribeListeners",
            "elasticloadbalancing:DescribeLoadBalancers"
          ],
          "Effect": "Allow",
          "Resource": "*",
          "Sid": "ElbAction1"
        }
      ],
      "Version": "2012-10-17"
    },
    "IsDefaultVersion": true,
    "VersionId": "v7"
  },
  "UpdateDate": "2023-05-19T20:41:25Z"
}
