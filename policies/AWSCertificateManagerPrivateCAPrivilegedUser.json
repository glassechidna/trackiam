{
  "Arn": "arn:aws:iam::aws:policy/AWSCertificateManagerPrivateCAPrivilegedUser",
  "CreateDate": "2019-06-20T17:43:13Z",
  "DefaultVersionId": "v4",
  "IsAttachable": true,
  "Path": "/",
  "PolicyId": "ANPAZKAPJZG4EQ6CWU5X5",
  "PolicyName": "AWSCertificateManagerPrivateCAPrivilegedUser",
  "PolicyVersion": {
    "CreateDate": "2026-02-12T18:00:06Z",
    "Document": {
      "Statement": [
        {
          "Action": [
            "acm-pca:IssueCertificate"
          ],
          "Condition": {
            "ArnLike": {
              "acm-pca:TemplateArn": [
                "arn:aws:acm-pca:*:*:template/*CACertificate*/V*"
              ]
            }
          },
          "Effect": "Allow",
          "Resource": "arn:aws:acm-pca:*:*:certificate-authority/*"
        },
        {
          "Action": [
            "acm-pca:IssueCertificate"
          ],
          "Condition": {
            "ArnNotLike": {
              "acm-pca:TemplateArn": [
                "arn:aws:acm-pca:*:*:template/*CACertificate*/V*"
              ]
            }
          },
          "Effect": "Deny",
          "Resource": "arn:aws:acm-pca:*:*:certificate-authority/*"
        },
        {
          "Action": [
            "acm-pca:GetCertificate",
            "acm-pca:ListPermissions",
            "acm-pca:RevokeCertificate"
          ],
          "Effect": "Allow",
          "Resource": "arn:aws:acm-pca:*:*:certificate-authority/*"
        },
        {
          "Action": [
            "acm-pca:ListCertificateAuthorities"
          ],
          "Effect": "Allow",
          "Resource": "*"
        }
      ],
      "Version": "2012-10-17"
    },
    "IsDefaultVersion": true,
    "VersionId": "v4"
  },
  "UpdateDate": "2026-02-12T18:00:06Z"
}
