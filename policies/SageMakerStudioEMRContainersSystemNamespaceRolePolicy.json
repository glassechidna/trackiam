{
  "Arn": "arn:aws:iam::aws:policy/service-role/SageMakerStudioEMRContainersSystemNamespaceRolePolicy",
  "CreateDate": "2025-10-23T18:34:06Z",
  "DefaultVersionId": "v1",
  "IsAttachable": true,
  "Path": "/service-role/",
  "PolicyId": "ANPAZKAPJZG4OYAGQIVA2",
  "PolicyName": "SageMakerStudioEMRContainersSystemNamespaceRolePolicy",
  "PolicyVersion": {
    "CreateDate": "2025-10-23T18:34:06Z",
    "Document": {
      "Statement": [
        {
          "Action": [
            "sts:AssumeRole",
            "sts:TagSession"
          ],
          "Condition": {
            "StringEquals": {
              "aws:CalledViaLast": "emr-containers.amazonaws.com",
              "aws:ResourceTag/AmazonDataZoneProject": "${aws:PrincipalTag/AmazonDataZoneProject}"
            },
            "StringEqualsIfExists": {
              "aws:RequestTag/AmazonDataZoneProject": "${aws:PrincipalTag/AmazonDataZoneProject}"
            }
          },
          "Effect": "Allow",
          "Resource": [
            "arn:aws:iam::*:role/datazone_emr_containers_query_engine_role_*",
            "arn:aws:iam::*:role/datazone_usr_role_*"
          ],
          "Sid": "AssumeProjectRoles"
        },
        {
          "Action": [
            "sts:SetContext"
          ],
          "Condition": {
            "StringEquals": {
              "aws:CalledViaLast": "emr-containers.amazonaws.com",
              "aws:ResourceTag/AmazonDataZoneProject": "${aws:PrincipalTag/AmazonDataZoneProject}"
            }
          },
          "Effect": "Allow",
          "Resource": [
            "arn:aws:iam::*:role/datazone_emr_containers_query_engine_role_*",
            "arn:aws:iam::*:role/datazone_usr_role_*"
          ],
          "Sid": "SetContextProjectRoles"
        }
      ],
      "Version": "2012-10-17"
    },
    "IsDefaultVersion": true,
    "VersionId": "v1"
  },
  "UpdateDate": "2025-10-23T18:34:06Z"
}
