{
  "Arn": "arn:aws:iam::aws:policy/aws-service-role/EC2FastLaunchServiceRolePolicy",
  "CreateDate": "2022-01-10T13:08:21Z",
  "DefaultVersionId": "v2",
  "IsAttachable": true,
  "Path": "/aws-service-role/",
  "PolicyId": "ANPAZKAPJZG4PBILMNSY7",
  "PolicyName": "EC2FastLaunchServiceRolePolicy",
  "PolicyVersion": {
    "CreateDate": "2025-05-14T17:07:07Z",
    "Document": {
      "Statement": [
        {
          "Action": "cloudwatch:PutMetricData",
          "Condition": {
            "StringEquals": {
              "cloudwatch:namespace": "AWS/EC2"
            }
          },
          "Effect": "Allow",
          "Resource": "*",
          "Sid": "AllowPutMetricData"
        },
        {
          "Action": "ec2:CreateLaunchTemplate",
          "Condition": {
            "StringEquals": {
              "aws:RequestTag/CreatedBy": "EC2 Fast Launch"
            }
          },
          "Effect": "Allow",
          "Resource": "arn:aws:ec2:*:*:launch-template/*",
          "Sid": "AllowCreateLaunchTemplate"
        },
        {
          "Action": "ec2:CreateSnapshot",
          "Condition": {
            "ForAnyValue:StringEquals": {
              "aws:TagKeys": [
                "CreatedByLaunchTemplateId",
                "CreatedByLaunchTemplateName"
              ]
            },
            "StringEquals": {
              "aws:RequestTag/CreatedBy": "EC2 Fast Launch"
            },
            "StringLike": {
              "aws:RequestTag/CreatedByLaunchTemplateVersion": "*"
            }
          },
          "Effect": "Allow",
          "Resource": [
            "arn:aws:ec2:*:*:snapshot/*"
          ],
          "Sid": "AllowCreateTaggedSnapshot"
        },
        {
          "Action": "ec2:CreateSnapshot",
          "Condition": {
            "StringEquals": {
              "aws:ResourceTag/CreatedBy": "EC2 Fast Launch"
            }
          },
          "Effect": "Allow",
          "Resource": [
            "arn:aws:ec2:*:*:volume/*"
          ],
          "Sid": "AllowCreateSnapshot"
        },
        {
          "Action": "ec2:CreateTags",
          "Condition": {
            "StringEquals": {
              "ec2:CreateAction": [
                "CreateLaunchTemplate",
                "CreateSnapshot",
                "RunInstances"
              ]
            }
          },
          "Effect": "Allow",
          "Resource": [
            "arn:aws:ec2:*:*:instance/*",
            "arn:aws:ec2:*:*:launch-template/*",
            "arn:aws:ec2:*:*:snapshot/*",
            "arn:aws:ec2:*:*:volume/*"
          ],
          "Sid": "AllowCreateTags"
        },
        {
          "Action": "iam:PassRole",
          "Condition": {
            "StringEquals": {
              "iam:PassedToService": [
                "ec2.amazonaws.com",
                "ec2.amazonaws.com.cn"
              ]
            }
          },
          "Effect": "Allow",
          "Resource": "*",
          "Sid": "AllowPassRole"
        },
        {
          "Action": [
            "ec2:DeleteLaunchTemplate"
          ],
          "Condition": {
            "StringEquals": {
              "aws:ResourceTag/CreatedBy": "EC2 Fast Launch"
            }
          },
          "Effect": "Allow",
          "Resource": "*",
          "Sid": "AllowDeleteFastLaunchLaunchTemplates"
        },
        {
          "Action": [
            "ec2:DeleteSnapshot"
          ],
          "Condition": {
            "StringEquals": {
              "aws:ResourceTag/CreatedBy": "EC2 Fast Launch"
            }
          },
          "Effect": "Allow",
          "Resource": [
            "arn:aws:ec2:*:*:snapshot/*"
          ],
          "Sid": "AllowDeleteSnapshots"
        },
        {
          "Action": [
            "ec2:RunInstances"
          ],
          "Condition": {
            "StringEquals": {
              "aws:RequestTag/CreatedBy": "EC2 Fast Launch"
            }
          },
          "Effect": "Allow",
          "Resource": [
            "arn:aws:ec2:*:*:instance/*",
            "arn:aws:ec2:*:*:volume/*"
          ],
          "Sid": "AllowRunInstancesOnFastLaunchCreatedResources"
        },
        {
          "Action": [
            "ec2:StopInstances",
            "ec2:TerminateInstances"
          ],
          "Condition": {
            "StringEquals": {
              "aws:ResourceTag/CreatedBy": "EC2 Fast Launch"
            }
          },
          "Effect": "Allow",
          "Resource": [
            "arn:aws:ec2:*:*:instance/*"
          ],
          "Sid": "AllowStopAndTerminateInstances"
        },
        {
          "Action": [
            "events:DeleteRule",
            "events:PutRule",
            "events:PutTargets",
            "events:RemoveTargets"
          ],
          "Condition": {
            "StringEquals": {
              "events:ManagedBy": "ec2fastlaunch.amazonaws.com"
            }
          },
          "Effect": "Allow",
          "Resource": [
            "arn:aws:events:*:*:rule/FastLaunch*"
          ],
          "Sid": "AllowEventsRuleMutations"
        },
        {
          "Action": "kms:ListRetirableGrants",
          "Effect": "Allow",
          "Resource": "*",
          "Sid": "AllowKMSActions"
        },
        {
          "Action": [
            "cloudformation:DescribeStacks",
            "ec2:DescribeImages",
            "ec2:DescribeInstanceAttribute",
            "ec2:DescribeInstanceStatus",
            "ec2:DescribeInstanceTypeOfferings",
            "ec2:DescribeInstances",
            "ec2:DescribeLaunchTemplateVersions",
            "ec2:DescribeLaunchTemplates",
            "ec2:DescribeSnapshots",
            "ec2:DescribeSubnets"
          ],
          "Effect": "Allow",
          "Resource": "*",
          "Sid": "AllowDescribeActions"
        },
        {
          "Action": [
            "ec2:RunInstances"
          ],
          "Effect": "Allow",
          "Resource": [
            "arn:aws:ec2:*:*:key-pair/*",
            "arn:aws:ec2:*:*:launch-template/*",
            "arn:aws:ec2:*:*:network-interface/*",
            "arn:aws:ec2:*:*:security-group/*",
            "arn:aws:ec2:*:*:subnet/*",
            "arn:aws:ec2:*::image/*",
            "arn:aws:license-manager:*:*:license-configuration:*"
          ],
          "Sid": "AllowRunInstances"
        },
        {
          "Action": [
            "events:DescribeRule",
            "events:ListTargetsByRule"
          ],
          "Effect": "Allow",
          "Resource": [
            "arn:aws:events:*:*:rule/FastLaunch*"
          ],
          "Sid": "AllowEventsRuleNonMutations"
        }
      ],
      "Version": "2012-10-17"
    },
    "IsDefaultVersion": true,
    "VersionId": "v2"
  },
  "UpdateDate": "2025-05-14T17:07:07Z"
}
