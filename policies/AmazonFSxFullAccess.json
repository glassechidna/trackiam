{
  "Arn": "arn:aws:iam::aws:policy/AmazonFSxFullAccess",
  "CreateDate": "2018-11-28T16:34:43Z",
  "DefaultVersionId": "v5",
  "IsAttachable": true,
  "Path": "/",
  "PolicyId": "ANPAIEUV6Z2X4VNZRVB5I",
  "PolicyName": "AmazonFSxFullAccess",
  "PolicyVersion": {
    "CreateDate": "2023-04-19T20:43:42Z",
    "Document": {
      "Statement": [
        {
          "Action": "iam:CreateServiceLinkedRole",
          "Condition": {
            "StringEquals": {
              "iam:AWSServiceName": [
                "fsx.amazonaws.com"
              ]
            }
          },
          "Effect": "Allow",
          "Resource": "*"
        },
        {
          "Action": "iam:CreateServiceLinkedRole",
          "Condition": {
            "StringEquals": {
              "iam:AWSServiceName": [
                "s3.data-source.lustre.fsx.amazonaws.com"
              ]
            }
          },
          "Effect": "Allow",
          "Resource": "*"
        },
        {
          "Action": [
            "ec2:CreateTags"
          ],
          "Condition": {
            "ForAnyValue:StringEquals": {
              "aws:CalledVia": [
                "fsx.amazonaws.com"
              ]
            },
            "StringEquals": {
              "aws:RequestTag/AmazonFSx": "ManagedByAmazonFSx"
            }
          },
          "Effect": "Allow",
          "Resource": [
            "arn:aws:ec2:*:*:route-table/*"
          ]
        },
        {
          "Action": [
            "ec2:DescribeRouteTables",
            "ec2:DescribeSecurityGroups",
            "ec2:DescribeSubnets",
            "ec2:DescribeVpcs"
          ],
          "Condition": {
            "ForAnyValue:StringEquals": {
              "aws:CalledVia": [
                "fsx.amazonaws.com"
              ]
            }
          },
          "Effect": "Allow",
          "Resource": "*"
        },
        {
          "Action": [
            "ds:DescribeDirectories",
            "fsx:*"
          ],
          "Effect": "Allow",
          "Resource": "*"
        },
        {
          "Action": [
            "firehose:PutRecord"
          ],
          "Effect": "Allow",
          "Resource": [
            "arn:aws:firehose:*:*:deliverystream/aws-fsx-*"
          ]
        },
        {
          "Action": [
            "logs:CreateLogGroup",
            "logs:CreateLogStream",
            "logs:PutLogEvents"
          ],
          "Effect": "Allow",
          "Resource": [
            "arn:aws:logs:*:*:log-group:/aws/fsx/*"
          ]
        }
      ],
      "Version": "2012-10-17"
    },
    "IsDefaultVersion": true,
    "VersionId": "v5"
  },
  "UpdateDate": "2023-04-19T20:43:42Z"
}
