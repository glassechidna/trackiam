{
  "Arn": "arn:aws:iam::aws:policy/AmazonFSxFullAccess",
  "CreateDate": "2018-11-28T16:34:43Z",
  "DefaultVersionId": "v8",
  "IsAttachable": true,
  "Path": "/",
  "PolicyId": "ANPAIEUV6Z2X4VNZRVB5I",
  "PolicyName": "AmazonFSxFullAccess",
  "PolicyVersion": {
    "CreateDate": "2023-11-27T05:55:28Z",
    "Document": {
      "Statement": [
        {
          "Action": "iam:CreateServiceLinkedRole",
          "Condition": {
            "StringEquals": {
              "iam:AWSServiceName": [
                "fsx.amazonaws.com"
              ]
            }
          },
          "Effect": "Allow",
          "Resource": "*",
          "Sid": "CreateSLRForFSx"
        },
        {
          "Action": "iam:CreateServiceLinkedRole",
          "Condition": {
            "StringEquals": {
              "iam:AWSServiceName": [
                "s3.data-source.lustre.fsx.amazonaws.com"
              ]
            }
          },
          "Effect": "Allow",
          "Resource": "*",
          "Sid": "CreateSLRForLustreS3Integration"
        },
        {
          "Action": [
            "ec2:CreateTags"
          ],
          "Condition": {
            "ForAnyValue:StringEquals": {
              "aws:CalledVia": [
                "fsx.amazonaws.com"
              ]
            },
            "StringEquals": {
              "aws:RequestTag/AmazonFSx": "ManagedByAmazonFSx"
            }
          },
          "Effect": "Allow",
          "Resource": [
            "arn:aws:ec2:*:*:route-table/*"
          ],
          "Sid": "CreateTags"
        },
        {
          "Action": [
            "ec2:DescribeRouteTables",
            "ec2:DescribeSecurityGroups",
            "ec2:DescribeSubnets",
            "ec2:DescribeVpcs"
          ],
          "Condition": {
            "ForAnyValue:StringEquals": {
              "aws:CalledVia": [
                "fsx.amazonaws.com"
              ]
            }
          },
          "Effect": "Allow",
          "Resource": "*",
          "Sid": "DescribeEC2VpcResources"
        },
        {
          "Action": [
            "ds:DescribeDirectories"
          ],
          "Effect": "Allow",
          "Resource": "*",
          "Sid": "ViewAWSDSDirectories"
        },
        {
          "Action": [
            "firehose:PutRecord"
          ],
          "Effect": "Allow",
          "Resource": [
            "arn:aws:firehose:*:*:deliverystream/aws-fsx-*"
          ],
          "Sid": "WriteToAmazonKinesisDataFirehose"
        },
        {
          "Action": [
            "fsx:AssociateFileGateway",
            "fsx:AssociateFileSystemAliases",
            "fsx:CancelDataRepositoryTask",
            "fsx:CopyBackup",
            "fsx:CopySnapshotAndUpdateVolume",
            "fsx:CreateBackup",
            "fsx:CreateDataRepositoryAssociation",
            "fsx:CreateDataRepositoryTask",
            "fsx:CreateFileCache",
            "fsx:CreateFileSystem",
            "fsx:CreateFileSystemFromBackup",
            "fsx:CreateSnapshot",
            "fsx:CreateStorageVirtualMachine",
            "fsx:CreateVolume",
            "fsx:CreateVolumeFromBackup",
            "fsx:DeleteBackup",
            "fsx:DeleteDataRepositoryAssociation",
            "fsx:DeleteFileCache",
            "fsx:DeleteFileSystem",
            "fsx:DeleteSnapshot",
            "fsx:DeleteStorageVirtualMachine",
            "fsx:DeleteVolume",
            "fsx:DescribeAssociatedFileGateways",
            "fsx:DescribeBackups",
            "fsx:DescribeDataRepositoryAssociations",
            "fsx:DescribeDataRepositoryTasks",
            "fsx:DescribeFileCaches",
            "fsx:DescribeFileSystemAliases",
            "fsx:DescribeFileSystems",
            "fsx:DescribeSharedVpcConfiguration",
            "fsx:DescribeSnapshots",
            "fsx:DescribeStorageVirtualMachines",
            "fsx:DescribeVolumes",
            "fsx:DisassociateFileGateway",
            "fsx:DisassociateFileSystemAliases",
            "fsx:ListTagsForResource",
            "fsx:ManageBackupPrincipalAssociations",
            "fsx:ReleaseFileSystemNfsV3Locks",
            "fsx:RestoreVolumeFromSnapshot",
            "fsx:TagResource",
            "fsx:UntagResource",
            "fsx:UpdateDataRepositoryAssociation",
            "fsx:UpdateFileCache",
            "fsx:UpdateFileSystem",
            "fsx:UpdateSharedVpcConfiguration",
            "fsx:UpdateSnapshot",
            "fsx:UpdateStorageVirtualMachine",
            "fsx:UpdateVolume"
          ],
          "Effect": "Allow",
          "Resource": "*",
          "Sid": "FullAccessToFSx"
        },
        {
          "Action": [
            "logs:CreateLogGroup",
            "logs:CreateLogStream",
            "logs:PutLogEvents"
          ],
          "Effect": "Allow",
          "Resource": [
            "arn:aws:logs:*:*:log-group:/aws/fsx/*"
          ],
          "Sid": "CreateLogsForFSxWindowsAuditLogs"
        }
      ],
      "Version": "2012-10-17"
    },
    "IsDefaultVersion": true,
    "VersionId": "v8"
  },
  "UpdateDate": "2023-11-27T05:55:28Z"
}
