{
  "Arn": "arn:aws:iam::aws:policy/aws-service-role/AWSLambdaServiceRolePolicy",
  "CreateDate": "2025-11-30T08:04:07Z",
  "DefaultVersionId": "v1",
  "IsAttachable": true,
  "Path": "/aws-service-role/",
  "PolicyId": "ANPAZKAPJZG4CUXRY3KLO",
  "PolicyName": "AWSLambdaServiceRolePolicy",
  "PolicyVersion": {
    "CreateDate": "2025-11-30T08:04:07Z",
    "Document": {
      "Statement": [
        {
          "Action": [
            "ec2:TerminateInstances"
          ],
          "Condition": {
            "StringEquals": {
              "ec2:ManagedResourceOperator": "scaler.lambda.amazonaws.com"
            }
          },
          "Effect": "Allow",
          "Resource": [
            "arn:aws:ec2:*:*:instance/*"
          ]
        },
        {
          "Action": [
            "ec2:DescribeInstanceStatus",
            "ec2:DescribeInstances"
          ],
          "Effect": "Allow",
          "Resource": [
            "*"
          ]
        }
      ],
      "Version": "2012-10-17"
    },
    "IsDefaultVersion": true,
    "VersionId": "v1"
  },
  "UpdateDate": "2025-11-30T08:04:07Z"
}
