{
  "Arn": "arn:aws:iam::aws:policy/AmazonPrometheusConsoleFullAccess",
  "CreateDate": "2020-12-15T18:11:10Z",
  "DefaultVersionId": "v5",
  "IsAttachable": true,
  "Path": "/",
  "PolicyId": "ANPAZKAPJZG4P7IR2JZ6H",
  "PolicyName": "AmazonPrometheusConsoleFullAccess",
  "PolicyVersion": {
    "CreateDate": "2025-05-22T17:07:06Z",
    "Document": {
      "Statement": [
        {
          "Action": [
            "aps:CreateAlertManagerDefinition",
            "aps:CreateLoggingConfiguration",
            "aps:CreateQueryLoggingConfiguration",
            "aps:CreateRuleGroupsNamespace",
            "aps:CreateWorkspace",
            "aps:DeleteAlertManagerDefinition",
            "aps:DeleteLoggingConfiguration",
            "aps:DeleteQueryLoggingConfiguration",
            "aps:DeleteRuleGroupsNamespace",
            "aps:DeleteWorkspace",
            "aps:DescribeAlertManagerDefinition",
            "aps:DescribeLoggingConfiguration",
            "aps:DescribeQueryLoggingConfiguration",
            "aps:DescribeRuleGroupsNamespace",
            "aps:DescribeWorkspace",
            "aps:DescribeWorkspaceConfiguration",
            "aps:ListRuleGroupsNamespaces",
            "aps:ListWorkspaces",
            "aps:PutAlertManagerDefinition",
            "aps:PutRuleGroupsNamespace",
            "aps:TagResource",
            "aps:UntagResource",
            "aps:UpdateLoggingConfiguration",
            "aps:UpdateQueryLoggingConfiguration",
            "aps:UpdateWorkspaceAlias",
            "aps:UpdateWorkspaceConfiguration"
          ],
          "Effect": "Allow",
          "Resource": "*"
        },
        {
          "Action": [
            "tag:GetTagKeys",
            "tag:GetTagValues"
          ],
          "Effect": "Allow",
          "Resource": "*"
        }
      ],
      "Version": "2012-10-17"
    },
    "IsDefaultVersion": true,
    "VersionId": "v5"
  },
  "UpdateDate": "2025-05-22T17:07:06Z"
}
