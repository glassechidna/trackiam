{
  "Arn": "arn:aws:iam::aws:policy/AmazonTimestreamConsoleFullAccess",
  "CreateDate": "2020-09-30T21:47:18Z",
  "DefaultVersionId": "v7",
  "IsAttachable": true,
  "Path": "/",
  "PolicyId": "ANPAZKAPJZG4AZJLUKMAZ",
  "PolicyName": "AmazonTimestreamConsoleFullAccess",
  "PolicyVersion": {
    "CreateDate": "2025-10-16T14:34:07Z",
    "Document": {
      "Statement": [
        {
          "Action": [
            "kms:CreateGrant"
          ],
          "Condition": {
            "Bool": {
              "kms:GrantIsForAWSResource": true
            },
            "ForAnyValue:StringEquals": {
              "kms:EncryptionContextKeys": "aws:timestream:database-name"
            },
            "StringEquals": {
              "aws:ResourceAccount": "${aws:PrincipalAccount}"
            },
            "StringLike": {
              "kms:ViaService": "timestream.*.amazonaws.com"
            }
          },
          "Effect": "Allow",
          "Resource": [
            "arn:aws:kms:*:*:key/*"
          ]
        },
        {
          "Action": [
            "aws-marketplace:AcceptAgreementRequest",
            "aws-marketplace:CreateAgreementRequest",
            "aws-marketplace:DescribeAgreement",
            "aws-marketplace:ListEntitlementDetails",
            "aws-marketplace:SearchAgreements",
            "aws-marketplace:Subscribe"
          ],
          "Condition": {
            "ForAnyValue:StringEquals": {
              "aws-marketplace:ProductId": [
                "prod-5jijo74ujy36m",
                "prod-rjppt7huo35fm",
                "prod-xcc5llpq4vlbc"
              ]
            }
          },
          "Effect": "Allow",
          "Resource": "*",
          "Sid": "AWSMarketplaceAccess"
        },
        {
          "Action": [
            "dbqms:CreateFavoriteQuery",
            "dbqms:CreateQueryHistory",
            "dbqms:DeleteFavoriteQueries",
            "dbqms:DeleteQueryHistory",
            "dbqms:DescribeFavoriteQueries",
            "dbqms:DescribeQueryHistory",
            "dbqms:GetQueryString",
            "dbqms:UpdateFavoriteQuery",
            "dbqms:UpdateQueryHistory"
          ],
          "Effect": "Allow",
          "Resource": "*"
        },
        {
          "Action": [
            "iam:ListRoles",
            "sns:ListTopics"
          ],
          "Effect": "Allow",
          "Resource": "*"
        },
        {
          "Action": [
            "kms:DescribeKey",
            "kms:ListAliases",
            "kms:ListKeys"
          ],
          "Effect": "Allow",
          "Resource": "*"
        },
        {
          "Action": [
            "pricing:GetProducts"
          ],
          "Effect": "Allow",
          "Resource": "*"
        },
        {
          "Action": [
            "s3:ListAllMyBuckets"
          ],
          "Effect": "Allow",
          "Resource": "*"
        },
        {
          "Action": [
            "timestream:*"
          ],
          "Effect": "Allow",
          "Resource": "*"
        },
        {
          "Action": [
            "aws-marketplace:ViewSubscriptions"
          ],
          "Effect": "Allow",
          "Resource": [
            "*"
          ],
          "Sid": "AWSMarketplaceViewSubscriptions"
        }
      ],
      "Version": "2012-10-17"
    },
    "IsDefaultVersion": true,
    "VersionId": "v7"
  },
  "UpdateDate": "2025-10-16T14:34:07Z"
}
