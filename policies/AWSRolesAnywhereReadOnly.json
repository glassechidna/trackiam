  "DefaultVersionId": "v1",
  "IsAttachable": true,
  "Path": "/",
  "PolicyId": "ANPAZKAPJZG4CS4GC5WVX",
  "PolicyName": "AWSRolesAnywhereReadOnly",
  "PolicyVersion": {
    "CreateDate": "2025-07-16T15:07:07Z",
    "Document": {
      "Statement": [
        {
          "Action": [
            "rolesanywhere:GetCrl",
            "rolesanywhere:ListCrls"
          ],
          "Effect": "Allow",
          "Resource": [
            "arn:aws:rolesanywhere:*:*:crl/*"
          ],
          "Sid": "CRLs"
        },
        {
          "Action": [
            "rolesanywhere:GetProfile",
            "rolesanywhere:ListProfiles"
          ],
          "Effect": "Allow",
          "Resource": [
            "arn:aws:rolesanywhere:*:*:profile/*"
          ],
          "Sid": "Profiles"
        },
        {
          "Action": [
            "rolesanywhere:GetSubject",
            "rolesanywhere:ListSubjects"
          ],
          "Effect": "Allow",
          "Resource": [
            "arn:aws:rolesanywhere:*:*:subject/*"
          ],
          "Sid": "Subjects"
        },
        {
          "Action": [
            "rolesanywhere:GetTrustAnchor",
            "rolesanywhere:ListTrustAnchors"
          ],
          "Effect": "Allow",
          "Resource": [
            "arn:aws:rolesanywhere:*:*:trust-anchor/*"
          ],
          "Sid": "TrustAnchors"
        },
        {
          "Action": [
            "rolesanywhere:ListTagsForResource"
          ],
          "Effect": "Allow",
          "Resource": [
            "*"
          ],
          "Sid": "Tags"
        }
      ],
      "Version": "2012-10-17"
    },
    "IsDefaultVersion": true,
    "VersionId": "v1"
